    },
    "40001010": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001010,
        "alias": "Contents.Quest.Guide.1010",
        "text": "(슬슬 내려가 볼까?)"
    },
    "40001011": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001011,
        "alias": "Contents.Quest.Guide.1011",
        "text": "(<textColor=[255:204:102:255]>무기</>를 골라보자!)"
    },
    "40001012": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드 (이동완료)",
        "id": 40001012,
        "alias": "Contents.Quest.Guide.1012",
        "text": "(사용할 <textColor=[255:204:102:255]>무기</>를 고르자!)"
    },
    "40001014": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드 (이동완료)",
        "id": 40001014,
        "alias": "Contents.Quest.Guide.1014",
        "text": "(목각인형을 상대로 \n연습해보자~!)"
    },
    "40001015": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001015,
        "alias": "Contents.Quest.Guide.1015",
        "text": "(음... 뭔가 부족한데?\n아껴둔 <textColor=[255:204:102:255]>진풍혼루</>를\n써야겠다)"
    },
    "40001017": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001017,
        "alias": "Contents.Quest.Guide.1017",
        "text": "(오~ 몸이 한층\n가벼워졌다~!)"
    },
    "40001019": {
        "deprecated": false,
        "Dev_Desc": "뭉게구름 가이드",
        "id": 40001019,
        "alias": "Contents.Quest.Guide.1019",
        "text": "선수 여러분~!\n준비 되셨나요오오~!"
    },
    "40001020": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001020,
        "alias": "Contents.Quest.Guide.1020",
        "text": "(결승전이 시작된다.\n<textColor=[110:216:175:255]>경기장</>으로 가자!)"
    },
    "40001021": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드 (이동완료)",
        "id": 40001021,
        "alias": "Contents.Quest.Guide.1021",
        "text": "(이 문을 나서면\n<textColor=[78:148:215:255]>아랑</>과의 결승전이다!)"
    },
    "40001022": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드 (이동완료)",
        "id": 40001022,
        "alias": "Contents.Quest.Guide.1022",
        "text": "(서로 최선을 다해\n멋진 시합을 하자!)"
    },
    "40001023": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001023,
        "alias": "Contents.Quest.Guide.1023",
        "text": "다행히 경계를 풀었어!"
    },
    "40001024": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001024,
        "alias": "Contents.Quest.Guide.1024",
        "text": "일단 저들의 <textColor=[78:148:215:255]>대장</>과\n얘기를 해보자."
    },
    "40001025": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001025,
        "alias": "Contents.Quest.Guide.1025",
        "text": "(대회를 망친\n책임을 져야 한다)"
    },
    "40001026": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001026,
        "alias": "Contents.Quest.Guide.1026",
        "text": "일단 여길 나가서\n<textColor=[110:216:175:255]>막사</>를 찾아보자!"
    },
    "40001027": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001027,
        "alias": "Contents.Quest.Guide.1027",
        "text": "<textColor=[255:204:102:255]><myHero.name/></>~!\n이쪽으로 \n나가면 되겠다!"
    },
    "40001028": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001028,
        "alias": "Contents.Quest.Guide.1028",
        "text": "(아랑을 따라\n경기장을 빠져나가자)"
    },
    "40001029": {
        "deprecated": false,
        "Dev_Desc": "동동 가이드",
        "id": 40001029,
        "alias": "Contents.Quest.Guide.1029",
        "text": "잠시만 기다려라냥!\n여기다냥!"
    },
    "40001030": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001030,
        "alias": "Contents.Quest.Guide.1030",
        "text": "(영교원 바롱 동동이 \n<textColor=[255:204:102:255]>소울 소환서</>를 줬다)"
    },
    "40001031": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001031,
        "alias": "Contents.Quest.Guide.1031",
        "text": "(기대되는데?\n어서 써봐야겠다!)"
    },
    "40001032": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001032,
        "alias": "Contents.Quest.Guide.1032",
        "text": "(벌써 든든한\n느낌이 든다~!)"
    },
    "40001033": {
        "deprecated": false,
        "Dev_Desc": "동동 가이드",
        "id": 40001033,
        "alias": "Contents.Quest.Guide.1033",
        "text": "방금 얻은 <textColor=[255:204:102:255]>소울</>은\n바로 쓸 수 있다냥~!"
    },
    "40001034": {
        "deprecated": false,
        "Dev_Desc": "동동 가이드",
        "id": 40001034,
        "alias": "Contents.Quest.Guide.1034",
        "text": "후냥후냥~ \n나는 간다냥~"
    },
    "40001035": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001035,
        "alias": "Contents.Quest.Guide.1035",
        "text": "(아랑을 따라 어서\n<textColor=[110:216:175:255]>에레다 막사</>로 가자)"
    },
    "40001036": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001036,
        "alias": "Contents.Quest.Guide.1036",
        "text": "<textColor=[255:204:102:255]><myHero.name/></>~\n왔구나~"
    },
    "40001037": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001037,
        "alias": "Contents.Quest.Guide.1037",
        "text": "(하름이 말한 <textColor=[78:148:215:255]>대원</>도\n저기 서있다)"
    },
    "40001038": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001038,
        "alias": "Contents.Quest.Guide.1038",
        "text": "(저 <textColor=[78:148:215:255]>도하</>라는 사람에게\n뭘 해야 될지 물어보자)"
    },
    "40001039": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001039,
        "alias": "Contents.Quest.Guide.1039",
        "text": "(<textColor=[78:148:215:255]>사호</>라는 자들을\n만나야 한다)"
    },
    "40001040": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001040,
        "alias": "Contents.Quest.Guide.1040",
        "text": "(일단 <textColor=[78:148:215:255]>아랑</>과 \n얘기해보자)"
    },
    "40001041": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001041,
        "alias": "Contents.Quest.Guide.1041",
        "text": "(동동과 비슷한\n차림의 바롱이 있다)"
    },
    "40001042": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001042,
        "alias": "Contents.Quest.Guide.1042",
        "text": "(여기가 <textColor=[110:216:175:255]>광장</>이\n맞는지 물어보자)"
    },
    "40001043": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001043,
        "alias": "Contents.Quest.Guide.1043",
        "text": "(<textColor=[255:204:102:255]>수호령</>이 대체 뭔지\n굉장히 궁금하다!)"
    },
    "40001044": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001044,
        "alias": "Contents.Quest.Guide.1044",
        "text": "(둥둥한테 받은\n<textColor=[255:204:102:255]>소환서</>를 사용해보자!)"
    },
    "40001045": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001045,
        "alias": "Contents.Quest.Guide.1045",
        "text": "(뭔가 굉장한 걸\n얻은 것 같다!)"
    },
    "40001046": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001046,
        "alias": "Contents.Quest.Guide.1046",
        "text": "(<textColor=[110:216:175:255]>광장</>은 여기서\n멀지 않다고 한다)"
    },
    "40001047": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001047,
        "alias": "Contents.Quest.Guide.1047",
        "text": "(어서 <textColor=[110:216:175:255]>광장</>으로 가\n아랑과 사호를 만나자)"
    },
    "40001048": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001048,
        "alias": "Contents.Quest.Guide.1048",
        "text": "앗 <textColor=[255:204:102:255]><myHero.name/></>!\n여기야 여기!"
    },
    "40001049": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001049,
        "alias": "Contents.Quest.Guide.1049",
        "text": "(아랑 곁의 사람들이\n<textColor=[78:148:215:255]>사호</>인 것 같다)"
    },
    "40001050": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001050,
        "alias": "Contents.Quest.Guide.1050",
        "text": "(<textColor=[78:148:215:255]>사호</>의 의뢰를 받았다)"
    },
    "40001051": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001051,
        "alias": "Contents.Quest.Guide.1051",
        "text": "(의뢰를 해결하고\n<textColor=[255:204:102:255]>유공상패</>를 획득하자!)"
    },
    "40001052": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001052,
        "alias": "Contents.Quest.Guide.1052",
        "text": "(사호의 의뢰를 해결하고\n<textColor=[255:204:102:255]>유공상패</>를 구해야 한다)"
    },
    "40001053": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001053,
        "alias": "Contents.Quest.Guide.1053",
        "text": "(<textColor=[255:204:102:255]>유공상패</> 네 장을\n모두 구했다!)"
    },
    "40001054": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001054,
        "alias": "Contents.Quest.Guide.1054",
        "text": "(<textColor=[110:216:175:255]>낚시꾼의 촌락</>으로 가자!)"
    },
    "40001058": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001058,
        "alias": "Contents.Quest.Guide.1058",
        "text": "앗, 떨고 있는 바롱이야!"
    },
    "40001059": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001059,
        "alias": "Contents.Quest.Guide.1059",
        "text": "(<textColor=[78:148:215:255]>영교원 약초꾼</> 중\n하나인 것 같다)"
    },
    "40001060": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001060,
        "alias": "Contents.Quest.Guide.1060",
        "text": "피부가 파란 사람이라면,\n아마 <textColor=[215:94:94:255]>청인족</>인 것 같아."
    },
    "40001061": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001061,
        "alias": "Contents.Quest.Guide.1061",
        "text": "어서 <textColor=[110:216:175:255]>난파선</>으로 가서\n바롱들을 구하자."
    },
    "40001062": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001062,
        "alias": "Contents.Quest.Guide.1062",
        "text": "앗! <textColor=[255:204:102:255]><myHero.name/></>!\n수상한 두루마리가 \n떨어져 있어!"
    },
    "40001063": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001063,
        "alias": "Contents.Quest.Guide.1063",
        "text": "상황이 \n급박해진 것 같아!"
    },
    "40001064": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001064,
        "alias": "Contents.Quest.Guide.1064",
        "text": "어서 <textColor=[110:216:175:255]>안쪽</>으로 가보자!"
    },
    "40001065": {
        "deprecated": false,
        "Dev_Desc": "링링 가이드",
        "id": 40001065,
        "alias": "Contents.Quest.Guide.1065",
        "text": "으앙~!! \n다들 무사했구나냥!!"
    },
    "40001066": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001066,
        "alias": "Contents.Quest.Guide.1066",
        "text": "(앗, <textColor=[78:148:215:255]>링링</>이 걱정돼서\n따라왔나 보다)"
    },
    "40001067": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001067,
        "alias": "Contents.Quest.Guide.1067",
        "text": "난파선 근처에 있는\n<textColor=[255:204:102:255]>반달초</>를 가져오자."
    },
    "40001068": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001068,
        "alias": "Contents.Quest.Guide.1068",
        "text": "다행히 <textColor=[215:94:94:255]>해적 하초</>는\n지금 자리를 비웠나 봐."
    },
    "40001069": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001069,
        "alias": "Contents.Quest.Guide.1069",
        "text": "(<textColor=[255:204:102:255]>반달초</>를 구했다~!)"
    },
    "40001070": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001070,
        "alias": "Contents.Quest.Guide.1070",
        "text": "(<textColor=[78:148:215:255]>링링</>에게 돌아가자!)"
    },
    "40001071": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001071,
        "alias": "Contents.Quest.Guide.1071",
        "text": "<textColor=[78:148:215:255]>퍼롱퍼롱</>에게 \n상황을 알려주자."
    },
    "40001072": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001072,
        "alias": "Contents.Quest.Guide.1072",
        "text": "영교원으로부터\n<textColor=[255:204:102:255]>유공상패</>를 얻었어!"
    },
    "40001075": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001075,
        "alias": "Contents.Quest.Guide.1075",
        "text": "(앗 임연수다! \n진짜 총타주이긴 한가보네)"
    },
    "40001076": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001076,
        "alias": "Contents.Quest.Guide.1076",
        "text": "(그런데 그리 신임이\n깊어 보이지는 않는군...)"
    },
    "40001077": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001077,
        "alias": "Contents.Quest.Guide.1077",
        "text": "(<textColor=[78:148:215:255]>심하연</>을 찾아보자)"
    },
    "40001078": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001078,
        "alias": "Contents.Quest.Guide.1078",
        "text": "귀중품은 내버려두고\n<textColor=[255:204:102:255]>식량</>만 훔친다고?"
    },
    "40001079": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001079,
        "alias": "Contents.Quest.Guide.1079",
        "text": "보통 도둑은 아닐 것 \n같은데? 일단 <textColor=[110:216:175:255]>목공의</> \n<textColor=[110:216:175:255]>거처</>로 가보자!"
    },
    "40001080": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001080,
        "alias": "Contents.Quest.Guide.1080",
        "text": "일단 <textColor=[255:204:102:255]>발자국</>을 \n따라가 보자."
    },
    "40001081": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001081,
        "alias": "Contents.Quest.Guide.1081",
        "text": "범인은 <textColor=[215:94:94:255]>서인족</>이었군."
    },
    "40001082": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001082,
        "alias": "Contents.Quest.Guide.1082",
        "text": "(남의 물건을 훔친 \n대가를 치르게 하자)"
    },
    "40001083": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001083,
        "alias": "Contents.Quest.Guide.1083",
        "text": "안쪽에 <textColor=[110:216:175:255]>서인족 근거지</>가 \n있는 것 같아."
    },
    "40001084": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001084,
        "alias": "Contents.Quest.Guide.1084",
        "text": "역시 더 많은 \n<textColor=[215:94:94:255]>서인족</>들이 있어."
    },
    "40001085": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001085,
        "alias": "Contents.Quest.Guide.1085",
        "text": "어떤 사고를 칠 지 \n모르니 처리하자!"
    },
    "40001086": {
        "deprecated": false,
        "Dev_Desc": "심하연 가이드",
        "id": 40001086,
        "alias": "Contents.Quest.Guide.1086",
        "text": "<textColor=[255:204:102:255]><myHero.name/></>님,\n이쪽입니다!"
    },
    "40001087": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001087,
        "alias": "Contents.Quest.Guide.1087",
        "text": "(앗, <textColor=[78:148:215:255]>심하연</>이다)"
    },
    "40001088": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001088,
        "alias": "Contents.Quest.Guide.1088",
        "text": "여기서 서쪽에 있는\n<textColor=[110:216:175:255]>호숫가 별채</>로 가보자."
    },
    "40001089": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001089,
        "alias": "Contents.Quest.Guide.1089",
        "text": "(<textColor=[255:204:102:255]>얼룩</>을 조사해보자)"
    },
    "40001090": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001090,
        "alias": "Contents.Quest.Guide.1090",
        "text": "붉은 얼룩은\n<textColor=[215:94:94:255]>핏자국</>인 것 같아!"
    },
    "40001091": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001091,
        "alias": "Contents.Quest.Guide.1091",
        "text": "<textColor=[110:216:175:255]>집 뒤편</>까지\n핏자국이 이어져 있어."
    },
    "40001092": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001092,
        "alias": "Contents.Quest.Guide.1092",
        "text": "(저 위험해 보이는 건 \n뭐지?!)"
    },
    "40001093": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001093,
        "alias": "Contents.Quest.Guide.1093",
        "text": "(일단 처치하자!)"
    },
    "40001094": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001094,
        "alias": "Contents.Quest.Guide.1094",
        "text": "(휴우... \n이 <textColor=[215:94:94:255]>괴물</>은 뭐지?)"
    },
    "40001095": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001095,
        "alias": "Contents.Quest.Guide.1095",
        "text": "(<textColor=[78:148:215:255]>심하연</>이 기다리고 있다)"
    },
    "40001096": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001096,
        "alias": "Contents.Quest.Guide.1096",
        "text": "매화단으로부터\n<textColor=[255:204:102:255]>유공상패</>를 얻었어!"
    },
    "40001099": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001099,
        "alias": "Contents.Quest.Guide.1099",
        "text": "저 사람이\n<textColor=[78:148:215:255]>곽유옥</>인가봐."
    },
    "40001100": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001100,
        "alias": "Contents.Quest.Guide.1100",
        "text": "무슨 일이 \n생긴 건지 물어보자."
    },
    "40001101": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001101,
        "alias": "Contents.Quest.Guide.1101",
        "text": "마차가 <textColor=[215:94:94:255]>습격</>을\n받았나 보군."
    },
    "40001102": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001102,
        "alias": "Contents.Quest.Guide.1102",
        "text": "<textColor=[255:204:102:255]>도둑맞은 화물</>을\n되찾아주자."
    },
    "40001103": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001103,
        "alias": "Contents.Quest.Guide.1103",
        "text": "다 회수한 것 같아!\n이제 돌아가자."
    },
    "40001104": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001104,
        "alias": "Contents.Quest.Guide.1104",
        "text": "여기 언덕 위가\n<textColor=[110:216:175:255]>자경단 초소</>야."
    },
    "40001105": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001105,
        "alias": "Contents.Quest.Guide.1105",
        "text": "가서 무슨 일인지\n알아보자."
    },
    "40001106": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001106,
        "alias": "Contents.Quest.Guide.1106",
        "text": "저 사람이 자경단\n<textColor=[78:148:215:255]>초소장</>인 것 같아."
    },
    "40001107": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001107,
        "alias": "Contents.Quest.Guide.1107",
        "text": "(앗! 순식간에 \n도망가버렸다)"
    },
    "40001108": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001108,
        "alias": "Contents.Quest.Guide.1108",
        "text": "뭔가 수상해.\n쫓아가자."
    },
    "40001109": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001109,
        "alias": "Contents.Quest.Guide.1109",
        "text": "갑자기 자경단이\n<textColor=[215:94:94:255]>공격</>해온다! \n조심해!"
    },
    "40001110": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001110,
        "alias": "Contents.Quest.Guide.1110",
        "text": "(휴우... \n겨우 제압했다)"
    },
    "40001111": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001111,
        "alias": "Contents.Quest.Guide.1111",
        "text": "초소장을 추궁해야겠어."
    },
    "40001112": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001112,
        "alias": "Contents.Quest.Guide.1112",
        "text": "(<textColor=[255:204:102:255]>윗선의 지시</>라...\n그게 누굴까?)"
    },
    "40001113": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001113,
        "alias": "Contents.Quest.Guide.1113",
        "text": "일단 이 <textColor=[255:204:102:255]>명령서</>를\n가지고 돌아가자."
    },
    "40001114": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001114,
        "alias": "Contents.Quest.Guide.1114",
        "text": "도화상전으로부터\n<textColor=[255:204:102:255]>유공상패</>를 얻었어!"
    },
    "40001117": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001117,
        "alias": "Contents.Quest.Guide.1117",
        "text": "이만하면 \n충분한 것 같아."
    },
    "40001118": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001118,
        "alias": "Contents.Quest.Guide.1118",
        "text": "근처에 있다는\n<textColor=[78:148:215:255]>강철주먹</>을 찾아보자."
    },
    "40001119": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001119,
        "alias": "Contents.Quest.Guide.1119",
        "text": "이 사람이\n<textColor=[78:148:215:255]>강철주먹</>인가봐."
    },
    "40001120": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001120,
        "alias": "Contents.Quest.Guide.1120",
        "text": "아무래도 이사람...\n짝사랑중인 것 같은데?"
    },
    "40001121": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001121,
        "alias": "Contents.Quest.Guide.1121",
        "text": "뭐, 알아서 하게 두고,\n<textColor=[110:216:175:255]>화물 보관소</>나 다녀오자!"
    },
    "40001122": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001122,
        "alias": "Contents.Quest.Guide.1122",
        "text": "여기가 <textColor=[110:216:175:255]>화물 보관소</>인가 봐."
    },
    "40001123": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001123,
        "alias": "Contents.Quest.Guide.1123",
        "text": "저기 <textColor=[78:148:215:255]>직원</>한테\n자초지종을 물어보자."
    },
    "40001124": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001124,
        "alias": "Contents.Quest.Guide.1124",
        "text": "와아... \n진짜 예의 없는 사람이다."
    },
    "40001125": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001125,
        "alias": "Contents.Quest.Guide.1125",
        "text": "일단 몰래 잠입해보자."
    },
    "40001126": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001126,
        "alias": "Contents.Quest.Guide.1126",
        "text": "앗, <textColor=[78:148:215:255]>직원들</>끼리\n무슨 얘기를 하고 있어."
    },
    "40001127": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001127,
        "alias": "Contents.Quest.Guide.1127",
        "text": "뒤로 가서\n살짝 엿들어보자."
    },
    "40001128": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001128,
        "alias": "Contents.Quest.Guide.1128",
        "text": "총타주의 <textColor=[255:204:102:255]>서신</>이라고?"
    },
    "40001129": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001129,
        "alias": "Contents.Quest.Guide.1129",
        "text": "직원들이 말한\n<textColor=[110:216:175:255]>장소</>로 가보자!"
    },
    "40001130": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001130,
        "alias": "Contents.Quest.Guide.1130",
        "text": "저 <textColor=[215:94:94:255]>용병들</>을 먼저\n처리해야겠군."
    },
    "40001131": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001131,
        "alias": "Contents.Quest.Guide.1131",
        "text": "<textColor=[255:204:102:255]><myHero.name/></>, 어서\n저 상자를 열어봐."
    },
    "40001132": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001132,
        "alias": "Contents.Quest.Guide.1132",
        "text": "정말 <textColor=[255:204:102:255]>서신</>이 있어?\n뭐라고 쓰여 있는 거야?"
    },
    "40001133": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001133,
        "alias": "Contents.Quest.Guide.1133",
        "text": "<textColor=[78:148:215:255]>초소장</>을 추궁해야겠어."
    },
    "40001134": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001134,
        "alias": "Contents.Quest.Guide.1134",
        "text": "솔직하지 못한\n사람이고만. 하하."
    },
    "40001135": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001135,
        "alias": "Contents.Quest.Guide.1135",
        "text": "금경철문으로부터\n<textColor=[255:204:102:255]>유공상패</>를 얻었어!"
    },
    "40001136": {
        "deprecated": false,
        "Dev_Desc": "뒷거래쥐 가이드",
        "id": 40001136,
        "alias": "Contents.Quest.Guide.1136",
        "text": "찍찍! 이봐라찍!"
    },
    "40001137": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001137,
        "alias": "Contents.Quest.Guide.1137",
        "text": "(앗! <textColor=[78:148:215:255]>수상한 쥐</>가\n손짓하고 있다)"
    },
    "40001138": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001138,
        "alias": "Contents.Quest.Guide.1138",
        "text": "방법은 상관없댔지?"
    },
    "40001139": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001139,
        "alias": "Contents.Quest.Guide.1139",
        "text": "돈만 충분하다면\n사는 것도 <textColor=[255:204:102:255]>방법</>이겠다!"
    },
    "40001140": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001140,
        "alias": "Contents.Quest.Guide.1140",
        "text": "엄청 비싸네.\n그래도 시간을 아꼈어!"
    },
    "40001146": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001146,
        "alias": "Contents.Quest.Guide.1146",
        "text": "(어서 가서\n<textColor=[78:148:215:255]>하름</>을 다시 만나자)"
    },
    "40001147": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001147,
        "alias": "Contents.Quest.Guide.1147",
        "text": "(하름에게 <textColor=[255:204:102:255]>유공상패</>를\n모두 모았다고 전하자!)"
    },
    "40001148": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001148,
        "alias": "Contents.Quest.Guide.1148",
        "text": "(무슨 일이지?\n따라가 보자)"
    },
    "40001149": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001149,
        "alias": "Contents.Quest.Guide.1149",
        "text": "(하름이 <textColor=[255:204:102:255]>신수</>였다니...)"
    },
    "40001150": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001150,
        "alias": "Contents.Quest.Guide.1150",
        "text": "(일단은 <textColor=[78:148:215:255]>아랑</>과\n뒷일을 상의해보자)"
    },
    "40001151": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001151,
        "alias": "Contents.Quest.Guide.1151",
        "text": "(아랑은 <textColor=[255:204:102:255]>비옥</>을\n찾으러 간다고 한다)"
    },
    "40001152": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001152,
        "alias": "Contents.Quest.Guide.1152",
        "text": "(오름은 나에게\n어떤 <textColor=[255:204:102:255]>의뢰</>를 하려나?)"
    },
    "40001153": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001153,
        "alias": "Contents.Quest.Guide.1153",
        "text": "(유공상패를 교환해주는\n<textColor=[255:204:102:255]>상인</>이 있다고 한다)"
    },
    "40001154": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001154,
        "alias": "Contents.Quest.Guide.1154",
        "text": "(<textColor=[255:204:102:255]>의뢰</>를 모두 수행했다)"
    },
    "40001155": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001155,
        "alias": "Contents.Quest.Guide.1155",
        "text": "(<textColor=[78:148:215:255]>하름</>을 찾아가자)"
    },
    "40001156": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001156,
        "alias": "Contents.Quest.Guide.1156",
        "text": "(가만히 기다리긴 \n뭐하다...)"
    },
    "40001157": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001157,
        "alias": "Contents.Quest.Guide.1157",
        "text": "(저 <textColor=[110:216:175:255]>입구</>를 조사해볼까?)"
    },
    "40001158": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001158,
        "alias": "Contents.Quest.Guide.1158",
        "text": "(이 <textColor=[255:204:102:255]>신비한 문</>은 뭐지?)"
    },
    "40001159": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001159,
        "alias": "Contents.Quest.Guide.1159",
        "text": "(마치 \n빨려 들어갈 것 같다)"
    },
    "40001160": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001160,
        "alias": "Contents.Quest.Guide.1160",
        "text": "(여긴 대체...)"
    },
    "40001161": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001161,
        "alias": "Contents.Quest.Guide.1161",
        "text": "(일단 <textColor=[110:216:175:255]>주변</>을 \n탐색해보자)"
    },
    "40001162": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001162,
        "alias": "Contents.Quest.Guide.1162",
        "text": "(<textColor=[215:94:94:255]>수호병</>들을\n처치하자)"
    },
    "40001163": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001163,
        "alias": "Contents.Quest.Guide.1163",
        "text": "(몸에 <textColor=[255:204:102:255]>새로운 힘</>이\n차는 느낌이다)"
    },
    "40001164": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001164,
        "alias": "Contents.Quest.Guide.1164",
        "text": "(새로운 <textColor=[255:204:102:255]>경공</>을\n사용할 수 있을 것 같다)"
    },
    "40001165": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001165,
        "alias": "Contents.Quest.Guide.1165",
        "text": "(물을 건너가 보자!)"
    },
    "40001166": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001166,
        "alias": "Contents.Quest.Guide.1166",
        "text": "(새로운 <textColor=[255:204:102:255]>수호령</>을 얻었다!)"
    },
    "40001167": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001167,
        "alias": "Contents.Quest.Guide.1167",
        "text": "(이제 이곳을 \n빠져나가자!)"
    },
    "40001168": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001168,
        "alias": "Contents.Quest.Guide.1168",
        "text": "(나갈 수 있게 됐다!)"
    },
    "40001169": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001169,
        "alias": "Contents.Quest.Guide.1169",
        "text": "(아랑이 말한\n<textColor=[110:216:175:255]>야영지</>를 찾아보자)"
    },
    "40001170": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001170,
        "alias": "Contents.Quest.Guide.1170",
        "text": "(아랑이 말한 \n장소가 여기다!)"
    },
    "40001171": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001171,
        "alias": "Contents.Quest.Guide.1171",
        "text": "(귀여운 바롱들이\n<textColor=[110:216:175:255]>야영지</>를 차리고 있다!)"
    },
    "40001172": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001172,
        "alias": "Contents.Quest.Guide.1172",
        "text": "(일단 <textColor=[215:94:94:255]>수호병</>들을 잡아보자!)"
    },
    "40001173": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001173,
        "alias": "Contents.Quest.Guide.1173",
        "text": "(더 안쪽으로 \n들어가 보자!)"
    },
    "40001174": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001174,
        "alias": "Contents.Quest.Guide.1174",
        "text": "(어디서 나타난 건지\n정말 놀랐다고 생각한다)"
    },
    "40001175": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001175,
        "alias": "Contents.Quest.Guide.1175",
        "text": "(반짝이는 게\n<textColor=[255:204:102:255]>비옥</>이 맞는지 살펴보자)"
    },
    "40001176": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001176,
        "alias": "Contents.Quest.Guide.1176",
        "text": "(그럼 다른 <textColor=[255:204:102:255]>비옥</>은\n쓸모 없는 거냐고 \n묻는다)"
    },
    "40001177": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001177,
        "alias": "Contents.Quest.Guide.1177",
        "text": "하하, 그런 건 아냐. \n일단 <textColor=[255:204:102:255]>비옥</>은 \n엄청나게 비싸. "
    },
    "40001178": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001178,
        "alias": "Contents.Quest.Guide.1178",
        "text": "보통 사람들이 \n눈에 불을 켜고 \n찾아다니는 이유이지. "
    },
    "40001179": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001179,
        "alias": "Contents.Quest.Guide.1179",
        "text": "그리고 이미 너도 \n알고 있겠지만\n<textColor=[215:94:94:255]>수라</> 같은 경우에는 "
    },
    "40001180": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001180,
        "alias": "Contents.Quest.Guide.1180",
        "text": "비옥의 <textColor=[255:204:102:255]>기운</>을 흡수해 \n자신의 힘과 생명을 \n연장해. "
    },
    "40001181": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001181,
        "alias": "Contents.Quest.Guide.1181",
        "text": "하지만 그들은 \n비옥 자체를 \n<textColor=[255:204:102:255]>직접 흡수</>할 순 없어서, "
    },
    "40001182": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001182,
        "alias": "Contents.Quest.Guide.1182",
        "text": "누군가 비옥에 이끌려 와서\n<textColor=[255:204:102:255]>봉인</>을 해제하는 걸 \n노리곤 하지."
    },
    "40001183": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001183,
        "alias": "Contents.Quest.Guide.1183",
        "text": "(좀 더 이야기를 \n듣고 싶다고 한다) "
    },
    "40001184": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001184,
        "alias": "Contents.Quest.Guide.1184",
        "text": "내 이야기가 \n재미있었나 보네? 하하"
    },
    "40001185": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001185,
        "alias": "Contents.Quest.Guide.1185",
        "text": "그렇지만 지금은 일단 \n조사를 먼저 마치자."
    },
    "40001186": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001186,
        "alias": "Contents.Quest.Guide.1186",
        "text": "(여기가 아랑이 말한\n<textColor=[110:216:175:255]>벼랑</>이다~!)"
    },
    "40001187": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001187,
        "alias": "Contents.Quest.Guide.1187",
        "text": "(<textColor=[110:216:175:255]>절벽 위</>를\n한 번 찾아보자!)"
    },
    "40001188": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001188,
        "alias": "Contents.Quest.Guide.1188",
        "text": "(앗! 저기 뭔가가\n반짝거리고 있다!)"
    },
    "40001189": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001189,
        "alias": "Contents.Quest.Guide.1189",
        "text": "저건... <textColor=[255:204:102:255]>청록색 비옥</>?"
    },
    "40001190": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001190,
        "alias": "Contents.Quest.Guide.1190",
        "text": "(나한테 들고 있는 걸\n주고 싶은 건가?)"
    },
    "40001191": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001191,
        "alias": "Contents.Quest.Guide.1191",
        "text": "<textColor=[255:204:102:255]><myHero.name/></>! 괜찮아??"
    },
    "40001192": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001192,
        "alias": "Contents.Quest.Guide.1192",
        "text": "(나도 비옥을\n흡수할 수 있다니 놀랍다)"
    },
    "40001193": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001193,
        "alias": "Contents.Quest.Guide.1193",
        "text": "(일단 공허의 구덩이로 축지하자!)"
    },
    "40001194": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001194,
        "alias": "Contents.Quest.Guide.1194",
        "text": "(저기 <textColor=[78:148:215:255]>오름</>이 \n기다리고 있다!)"
    },
    "40001195": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001195,
        "alias": "Contents.Quest.Guide.1195",
        "text": "(<textColor=[255:204:102:255]>불마자의 핵</>을\n수집해야 한다)"
    },
    "40001196": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001196,
        "alias": "Contents.Quest.Guide.1196",
        "text": "(누구 물어볼 사람\n없을까?)"
    },
    "40001197": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001197,
        "alias": "Contents.Quest.Guide.1197",
        "text": "(저 사람에게\n<textColor=[215:94:94:255]>불마자</>에 관해 물어보자!)"
    },
    "40001198": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001198,
        "alias": "Contents.Quest.Guide.1198",
        "text": "(그럼 <textColor=[215:94:94:255]>불마자</>를\n사냥해볼까?)"
    },
    "40001200": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001200,
        "alias": "Contents.Quest.Guide.1200",
        "text": "(<textColor=[255:204:102:255]>돈을 기부</>하고 \n살 수도 있는 것 같다)"
    },
    "40001202": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001202,
        "alias": "Contents.Quest.Guide.1202",
        "text": "(<textColor=[215:94:94:255]>쇠약한 불마자</>에게서\n<textColor=[255:204:102:255]>온전한 핵</>을 모으자)"
    },
    "40001203": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001203,
        "alias": "Contents.Quest.Guide.1203",
        "text": "(일단, <textColor=[215:94:94:255]>불마자</>를 잡으면 \n나오는 것 같고)"
    },
    "40001205": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001205,
        "alias": "Contents.Quest.Guide.1205",
        "text": "(<textColor=[255:204:102:255]>돈을 기부</>하고 \n살 수도 있는 것 같다)"
    },
    "40001206": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001206,
        "alias": "Contents.Quest.Guide.1206",
        "text": "(무사히 하름의\n과제를 완료했다)"
    },
    "40001207": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001207,
        "alias": "Contents.Quest.Guide.1207",
        "text": "(<textColor=[78:148:215:255]>하름</>이 보인다.\n가서 결과를 알려주자)"
    },
    "40001208": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001208,
        "alias": "Contents.Quest.Guide.1208",
        "text": "(<textColor=[78:148:215:255]>아랑</>은 나중에\n따라온다고 한다)"
    },
    "40001209": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001209,
        "alias": "Contents.Quest.Guide.1209",
        "text": "(하름이 알려준\n<textColor=[110:216:175:255]>깊은 묘지 숲</>으로 가자)"
    },
    "40001210": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001210,
        "alias": "Contents.Quest.Guide.1210",
        "text": "(숲을 어지럽히는\n존재들을 정리하자)"
    },
    "40001211": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001211,
        "alias": "Contents.Quest.Guide.1211",
        "text": "(<textColor=[215:94:94:255]>괴물들</>이 어느 정도\n정리됐다)"
    },
    "40001212": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001212,
        "alias": "Contents.Quest.Guide.1212",
        "text": "(안쪽으로 가\n<textColor=[78:148:215:255]>하름</>을 만나자)"
    },
    "40001214": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001214,
        "alias": "Contents.Quest.Guide.1214",
        "text": "(<textColor=[255:204:102:255]>토모의 비옥</>을\n흡수해보자)"
    },
    "40001215": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001215,
        "alias": "Contents.Quest.Guide.1215",
        "text": "(열린 문 안쪽에\n<textColor=[215:94:94:255]>더 큰 수호병</>이 있다!)"
    },
    "40001216": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001216,
        "alias": "Contents.Quest.Guide.1216",
        "text": "(눈 앞에 <textColor=[110:216:175:255]>신비한 장치</>가 나타났다!)"
    },
    "40001217": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001217,
        "alias": "Contents.Quest.Guide.1217",
        "text": "(저 사람이 에레다 도하인가보다)"
    },
    "40001218": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001218,
        "alias": "Contents.Quest.Guide.1218",
        "text": "(앗! 총타에서 만났던\n에레다 대원 도하가 서있다)"
    },
    "40001219": {
        "deprecated": false,
        "Dev_Desc": "반달 에픽 이름",
        "id": 40001219,
        "alias": "Contents.Quest.Guide.1219",
        "text": "매화단 단장 홍매화"
    },
    "40001220": {
        "deprecated": false,
        "Dev_Desc": "홍매화 가이드",
        "id": 40001220,
        "alias": "Contents.Quest.Guide.1220",
        "text": "잠시, <textColor=[255:204:102:255]><myHero.name/></>님.\n더 드릴 말씀이 있습니다."
    },
    "40001221": {
        "deprecated": false,
        "Dev_Desc": "심하연 가이드",
        "id": 40001221,
        "alias": "Contents.Quest.Guide.1221",
        "text": "<textColor=[255:204:102:255]><myHero.name/></>님. 잠시만요~"
    },
    "40001222": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001222,
        "alias": "Contents.Quest.Guide.1222",
        "text": "(드디어 홍린의 핵을 구했다!\n축지를 사용해 수월항으로 돌아가자!)"
    },
    "40001223": {
        "deprecated": false,
        "Dev_Desc": "뭉게구름 가이드",
        "id": 40001223,
        "alias": "Contents.Quest.Guide.1223",
        "text": "여기요! 잠시만요!\n드릴 말씀이 있어요!"
    },
    "40001224": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001224,
        "alias": "Contents.Quest.Guide.1224",
        "text": "(결승 진출자를 위한\n반달호수 주민들의\n응원 선물이 있다고 한다~!)"
    },
    "40001225": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001225,
        "alias": "Contents.Quest.Guide.1225",
        "text": "(드러난 유적지로\n축지를 사용하면\n더 빨리 갈 수 있겠다~!)"
    },
    "40001226": {
        "deprecated": false,
        "Dev_Desc": "임연수 가이드",
        "id": 40001226,
        "alias": "Contents.Quest.Guide.1226",
        "text": "멍청아 약효 떨어진다고!\n당장 경기장으로 가!!!"
    },
    "40001227": {
        "deprecated": false,
        "Dev_Desc": "임연수 가이드",
        "id": 40001227,
        "alias": "Contents.Quest.Guide.1227",
        "text": "대어방 총타주 임연수"
    },
    "40001228": {
        "deprecated": false,
        "Dev_Desc": "심하연 가이드",
        "id": 40001228,
        "alias": "Contents.Quest.Guide.1228",
        "text": "소협! 늦어서 죄송합니다!\n이게 다 어찌된 일인가요?"
    },
    "40001229": {
        "deprecated": false,
        "Dev_Desc": "임연수 가이드",
        "id": 40001229,
        "alias": "Contents.Quest.Guide.1229",
        "text": "자 가라 나의 사병들아!!\n크하하하!!"
    },
    "40001230": {
        "deprecated": false,
        "Dev_Desc": "임연수 가이드",
        "id": 40001230,
        "alias": "Contents.Quest.Guide.1230",
        "text": "뭣들 하는 거야!?\n당장 저 놈을 죽이란 말야!!"
    },
    "40001231": {
        "deprecated": false,
        "Dev_Desc": "임연수 가이드",
        "id": 40001231,
        "alias": "Contents.Quest.Guide.1231",
        "text": "크윽... 모... 몸이...\n몸이 좀 이상한데?!"
    },
    "40001232": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001232,
        "alias": "Contents.Quest.Guide.1232",
        "text": "(맙소사 <textColor=[215:94:94:255]>임연수</>가 대체\n무슨 일을 벌인 거야?!!!)"
    },
    "40001233": {
        "deprecated": false,
        "Dev_Desc": "임연수 가이드",
        "id": 40001233,
        "alias": "Contents.Quest.Guide.1233",
        "text": "야! 거기 너 임마!\n당장 이리로 튀어와!"
    },
    "40001234": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001234,
        "alias": "Contents.Quest.Guide.1234",
        "text": "(정예 서인족들도\n혼쭐내주자!)"
    },
    "40001235": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001235,
        "alias": "Contents.Quest.Guide.1235",
        "text": "(생선 도둑도치들도\n화물을 훔쳐간 것 같다)"
    },
    "40001236": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001236,
        "alias": "Contents.Quest.Guide.1236",
        "text": "(꽃게 도둑도치들이\n가져간 화물도\n회수해오자~!)"
    },
    "40001237": {
        "deprecated": false,
        "Dev_Desc": "아랑 가이드",
        "id": 40001237,
        "alias": "Contents.Quest.Guide.1237",
        "text": "<textColor=[255:204:102:255]><myHero.name/></>~!\n이쪽이야~"
    },
    "40001238": {
        "deprecated": false,
        "Dev_Desc": "PC 가이드",
        "id": 40001238,
        "alias": "Contents.Quest.Guide.1238",
        "text": "(아랑도 곧 도착할 것 같다)"
    }
}