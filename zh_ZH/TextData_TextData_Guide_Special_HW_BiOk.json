    "40045041": {
        "deprecated": false,
        "Dev_Desc": "비옥 스페셜 캐릭터 이름",
        "id": 40045041,
        "alias": "Contents.Quest.Guide.45041",
        "text": "画前的张久里"
    },
    "40045042": {
        "deprecated": false,
        "Dev_Desc": "비옥 스페셜 캐릭터 이름",
        "id": 40045042,
