{
    "40040000": {
        "deprecated": false,
        "Dev_Desc": "홍돈칠칠",
        "id": 40040000,
        "alias": "Contents.Quest.Guide.40000",
        "text": "我不会手下留情～！"
    },
    "40040001": {
        "deprecated": false,
        "Dev_Desc": "아랑",
        "id": 40040001,
        "alias": "Contents.Quest.Guide.40001",
        "text": "这样就够了吧？"
    },
    "40040002": {
        "deprecated": false,
        "Dev_Desc": "아랑",
        "id": 40040002,
        "alias": "Contents.Quest.Guide.40002",
        "text": "好，去那里等吧。"
    },
    "40040003": {
        "deprecated": false,
        "Dev_Desc": "아랑",
        "id": 40040003,
        "alias": "Contents.Quest.Guide.40003",
        "text": "快走吧！"
    },
    "40040004": {
        "deprecated": false,
        "Dev_Desc": "홍도니",
        "id": 40040004,
        "alias": "Contents.Quest.Guide.40004",
        "text": "这边～！"
    },
    "40040005": {
        "deprecated": false,
        "Dev_Desc": "홍돈칠칠의 부하",
        "id": 40040005,
        "alias": "Contents.Quest.Guide.40005",
        "text": "谢谢～！！"
    },
    "40040006": {
        "deprecated": false,
        "Dev_Desc": "아랑",
        "id": 40040006,
        "alias": "Contents.Quest.Guide.40006",
        "text": "这样就够了吧？\n回<textColor=[78:148:215:255]>风白门</>吧。"
    },
    "40040007": {
        "deprecated": false,
        "Dev_Desc": "아랑",
        "id": 40040007,
        "alias": "Contents.Quest.Guide.40007",
        "text": "接下来是<textColor=[215:94:94:255]>黑暗黎明团</>？"
    },
    "40040008": {
        "deprecated": false,
        "Dev_Desc": "아랑",
        "id": 40040008,
        "alias": "Contents.Quest.Guide.40008",
        "text": "去找其他组长吧。"
    },
    "40040009": {
        "deprecated": false,
        "Dev_Desc": "사로잡힌 전투조원",
        "id": 40040009,
        "alias": "Contents.Quest.Guide.40009",
        "text": "门主大人！谢谢！"
    },
    "40040010": {
        "deprecated": false,
        "Dev_Desc": "아랑",
        "id": 40040010,
        "alias": "Contents.Quest.Guide.40010",
        "text": "嗯？这不是<textColor=[78:148:215:255]>时局师兄</>吗？"
    },
    "40040011": {
        "deprecated": false,
        "Dev_Desc": "PC",
        "id": 40040011,
        "alias": "Contents.Quest.Guide.40011",
        "text": "（差不多可以回去了。）"
    },
    "40040012": {
        "deprecated": false,
        "Dev_Desc": "아랑",
        "id": 40040012,
        "alias": "Contents.Quest.Guide.40012",
        "text": "先了解一下情况。"
    },
    "40040013": {
        "deprecated": false,
        "Dev_Desc": "아랑",
        "id": 40040013,
        "alias": "Contents.Quest.Guide.40013",
        "text": "<textColor=[215:94:94:255]>不磨者</>竟然…\n<textColor=[78:148:215:255]>时局师兄</>不会有事吧？"
    },
    "40040014": {
        "deprecated": false,
        "Dev_Desc": "아랑",
        "id": 40040014,
        "alias": "Contents.Quest.Guide.40014",
        "text": "<textColor=[78:148:215:255]>时局师兄</>…到底…\n在做什么…"
    },
    "40040015": {
        "deprecated": false,
        "Dev_Desc": "아랑",
        "id": 40040015,
        "alias": "Contents.Quest.Guide.40015",
        "text": "那边有一个强大的家伙！"
    },
    "40040016": {
        "deprecated": false,
        "Dev_Desc": "PC",
        "id": 40040016,
        "alias": "Contents.Quest.Guide.40016",
        "text": "（在初来乍到的地方搞什么轻功对决…）"
    },
    "40040017": {
        "deprecated": false,
        "Dev_Desc": "PC",
        "id": 40040017,
        "alias": "Contents.Quest.Guide.40017",
        "text": "（先沿着路走下去吧。）"
    },
    "40040018": {
        "deprecated": false,
        "Dev_Desc": "아랑",
        "id": 40040018,
        "alias": "Contents.Quest.Guide.40018",
        "text": "为什么来得这么晚～"
    },
    "40040019": {
        "deprecated": false,
        "Dev_Desc": "아랑",
        "id": 40040019,
        "alias": "Contents.Quest.Guide.40019",
        "text": "看来需要重新找<textColor=[78:148:215:255]>百草老头</>学一次！"
    },
    "40040020": {
        "deprecated": false,
        "Dev_Desc": "아랑",
        "id": 40040020,
        "alias": "Contents.Quest.Guide.40020",
        "text": "还差得远了～"
    },
    "40040021": {
        "deprecated": false,
        "Dev_Desc": "PC",
        "id": 40040021,
        "alias": "Contents.Quest.Guide.40021",
        "text": "（看不到<textColor=[78:148:215:255]>饿狼</>。）"
    },
    "40040022": {
        "deprecated": false,
        "Dev_Desc": "PC",
        "id": 40040022,
        "alias": "Contents.Quest.Guide.40022",
        "text": "（应该是往这边走了。）"
    },
    "40040023": {
        "deprecated": false,
        "Dev_Desc": "아랑",
        "id": 40040023,
        "alias": "Contents.Quest.Guide.40023",
        "text": "就是这里，这里！"
    },
    "40040024": {
        "deprecated": false,
        "Dev_Desc": "홍돈칠칠",
        "id": 40040024,
        "alias": "Contents.Quest.Guide.40024",
        "text": "血猪七"
    },
    "40040025": {
        "deprecated": false,
        "Dev_Desc": "아랑",
        "id": 40040025,
        "alias": "Contents.Quest.Guide.40025",
        "text": "饿狼"
    },
    "40040026": {
        "deprecated": false,
        "Dev_Desc": "홍도니",
        "id": 40040026,
        "alias": "Contents.Quest.Guide.40026",
        "text": "红敦"
    },
    "40040027": {
        "deprecated": false,
        "Dev_Desc": "홍마니",
        "id": 40040027,
        "alias": "Contents.Quest.Guide.40027",
        "text": "红万"
    },
    "40040028": {
        "deprecated": false,
        "Dev_Desc": "홍처리",
        "id": 40040028,
        "alias": "Contents.Quest.Guide.40028",
        "text": "红喆"
    },
    "40040029": {
        "deprecated": false,
        "Dev_Desc": "사로잡힌 전투조원",
        "id": 40040029,
        "alias": "Contents.Quest.Guide.40029",
        "text": "风白门战斗组员"
    },
    "40040030": {
        "deprecated": false,
        "Dev_Desc": "아랑",
        "id": 40040030,
        "alias": "Contents.Quest.Guide.40030",
        "text": "目前为止\n刚来难吗?"
    },
    "40040031": {
        "deprecated": false,
        "Dev_Desc": "아랑",
        "id": 40040031,
        "alias": "Contents.Quest.Guide.40031",
        "text": "一边整理环境\n,一边来!"
    },
    "40040032": {
        "deprecated": false,
        "Dev_Desc": "검은새벽단 강우",
        "id": 40040032,
        "alias": "Contents.Quest.Guide.40032",
        "text": "你是个什么样的人!我一路无所畏惧地来到这里.不要让我后悔!"
    },
    "40040033": {
        "deprecated": false,
        "Dev_Desc": "PC",
        "id": 40040033,
        "alias": "Contents.Quest.Guide.40033",
        "text": "(据说俘虏是\n某处.\n去找他们吧)"
    },
    "40040034": {
        "deprecated": false,
        "Dev_Desc": "PC",
        "id": 40040034,
        "alias": "Contents.Quest.Guide.40034",
        "text": "(我必须在走之前杀死<textColor=[215:94:94:255]>黑色黎明</>!)"
    },
    "40040035": {
        "deprecated": false,
        "Dev_Desc": "검은새벽단 강우",
        "id": 40040035,
        "alias": "Contents.Quest.Guide.40035",
        "text": "黑色黎明降雨"
    },
    "40040036": {
        "deprecated": false,
        "Dev_Desc": "아랑",
        "id": 40040036,
        "alias": "Contents.Quest.Guide.40036",
        "text": "你是他们的队长吗!? \n 干掉它吧!"
    },
    "40040037": {
        "deprecated": false,
        "Dev_Desc": "아랑",
        "id": 40040037,
        "alias": "Contents.Quest.Guide.40037",
        "text": "就是这个!"
    }
}