    "40020023": {
        "deprecated": false,
        "Dev_Desc": "도화상전 책임자",
        "id": 40020023,
        "alias": "Contents.Quest.Guide.20022",
        "text": "接下来是锅。"
    },
    "40020024": {
        "deprecated": false,
        "Dev_Desc": "도화상전 책임자",
        "id": 40020024,
        "alias": "Contents.Quest.Guide.20023",
        "text": "请准备一下<textColor=[255:204:102:255]>柴火</>。"
    },
    "40020025": {
        "deprecated": false,
        "Dev_Desc": "도화상전 책임자",
        "id": 40020025,
        "alias": "Contents.Quest.Guide.20024",
        "text": "然后生火吧。"
    },
    "40020026": {
        "deprecated": false,
        "Dev_Desc": "도화상전 책임자",
        "id": 40020026,
        "alias": "Contents.Quest.Guide.20025",
        "text": "好了。现在只要等待就行了。"
    },
    "40020027": {
        "deprecated": false,
        "Dev_Desc": "문주 달빛바람",
        "id": 40020027,
        "alias": "Contents.Quest.Guide.20026",
        "text": "首先是<textColor=[255:204:102:255]>暗角大鬼的角</>。"
    },
    "40020028": {
        "deprecated": false,
        "Dev_Desc": "문주 달빛바람",
        "id": 40020028,
        "alias": "Contents.Quest.Guide.20027",
        "text": "其次是<textColor=[255:204:102:255]>赤匪派的武器</>。"
    },
    "40020029": {
        "deprecated": false,
        "Dev_Desc": "문주 달빛바람",
        "id": 40020029,
        "alias": "Contents.Quest.Guide.20028",
        "text": "最后一个是，<textColor=[255:204:102:255]>幻影魂火</>！"
    },
    "40020030": {
        "deprecated": false,
        "Dev_Desc": "문주 달빛바람",
        "id": 40020030,
        "alias": "Contents.Quest.Guide.20029",
        "text": "<textColor=[255:204:102:255]>幻影魂火</>，总觉得很微妙。\n那么，在据点见。"
    },
    "40020031": {
        "deprecated": false,
        "Dev_Desc": "영교원 동동",
        "id": 40020031,
        "alias": "Contents.Quest.Guide.20030",
        "text": "咦？你们！不认识我喵？"
    },
    "40020032": {
        "deprecated": false,
        "Dev_Desc": "PC",
        "id": 40020032,
        "alias": "Contents.Quest.Guide.20031",
        "text": "（唉，我的头…）"
    },
    "40020033": {
        "deprecated": false,
        "Dev_Desc": "어린 바롱",
        "id": 40020033,
        "alias": "Contents.Quest.Guide.20032",
        "text": "来帮帮我喵！"
    },
    "40020034": {
        "deprecated": false,
        "Dev_Desc": "어린 바롱",
        "id": 40020034,
        "alias": "Contents.Quest.Guide.20033",
        "text": "还剩下一些喵！"
    },
    "40020035": {
        "deprecated": false,
        "Dev_Desc": "어린 바롱",
        "id": 40020035,
        "alias": "Contents.Quest.Guide.20034",
        "text": "这样就好了喵。"
    },
    "40020036": {
        "deprecated": false,
        "Dev_Desc": "어린 바롱",
        "id": 40020036,
        "alias": "Contents.Quest.Guide.20035",
        "text": "跟我走喵～"
    },
    "40020037": {
        "deprecated": false,
        "Dev_Desc": "어린 바롱",
        "id": 40020037,
        "alias": "Contents.Quest.Guide.20036",
        "text": "在那里喵～！"
    },
    "40020038": {
        "deprecated": false,
        "Dev_Desc": "PC",
        "id": 40020038,
        "alias": "Contents.Quest.Guide.20037",
        "text": "（这里是办公室…？\n仔细查看一下吧。）"
    },
    "40020039": {
        "deprecated": false,
        "Dev_Desc": "하름",
        "id": 40020039,
        "alias": "Contents.Quest.Guide.20038",
        "text": "在找我吗？"
    },
    "40020040": {
        "deprecated": false,
        "Dev_Desc": "PC",
        "id": 40020040,
        "alias": "Contents.Quest.Guide.20039",
        "text": "（找一下安静的地方吧。）"
    },
    "40020041": {
        "deprecated": false,
        "Dev_Desc": "봇짐상인",
        "id": 40020041,
        "alias": "Contents.Quest.Guide.20040",
        "text": "喂，有人吗？！\n请救救我！"
