    "40020042": {
        "deprecated": false,
        "Dev_Desc": "봇짐상인",
        "id": 40020042,
        "alias": "Contents.Quest.Guide.20041",
        "text": "多谢救命之恩。"
    },
    "40020043": {
        "deprecated": false,
        "Dev_Desc": "PC",
        "id": 40020043,
        "alias": "Contents.Quest.Guide.20042",
        "text": "（去那边看看吧。）"
    },
    "40020044": {
        "deprecated": false,
        "Dev_Desc": "PC",
        "id": 40020044,
        "alias": "Contents.Quest.Guide.20043",
        "text": "（去里面看看吧。）"
    },
    "40020045": {
        "deprecated": false,
        "Dev_Desc": "PC",
        "id": 40020045,
        "alias": "Contents.Quest.Guide.20044",
        "text": "（路开了。）"
    },
    "40020046": {
        "deprecated": false,
        "Dev_Desc": "아랑",
        "id": 40020046,
        "alias": "Contents.Quest.Guide.20045",
        "text": "这样就可以了吧？"
    },
    "40020047": {
        "deprecated": false,
        "Dev_Desc": "???",
        "id": 40020047,
        "alias": "Contents.Quest.Guide.20046",
        "text": "饿狼，请来这边。"
    },
    "40020048": {
        "deprecated": false,
        "Dev_Desc": "???",
        "id": 40020048,
        "alias": "Contents.Quest.Guide.20047",
        "text": "好久不见。"
    },
    "40020049": {
        "deprecated": false,
        "Dev_Desc": "거점책임자",
        "id": 40020049,
        "alias": "Contents.Quest.Guide.20048",
        "text": "只要你找到标本，\n我会去啖人草湿地。"
    },
    "40020050": {
        "deprecated": false,
        "Dev_Desc": "아랑",
        "id": 40020050,
        "alias": "Contents.Quest.Guide.20049",
        "text": "感觉还不够吧？\n以强大的家伙们为主消灭吧。"
    },
    "40020051": {
        "deprecated": false,
        "Dev_Desc": "PC",
        "id": 40020051,
        "alias": "Contents.Quest.Guide.20052",
        "text": "（去找据点负责人吧。）"
    },
    "40020052": {
        "deprecated": false,
        "Dev_Desc": "아랑",
        "id": 40020052,
        "alias": "Contents.Quest.Guide.20053",
