    },
    "40020022": {
        "deprecated": false,
        "Dev_Desc": "도화상전 일꾼",
        "id": 40020022,
        "alias": "Contents.Quest.Guide.20021",
        "text": "非常感谢！\n多亏了少侠，我们可以回去了！"
