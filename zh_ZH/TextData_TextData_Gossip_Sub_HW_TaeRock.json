    "30030011": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30201_0_수양대원 목치본",
        "id": 30030011,
        "alias": "Contents.Gossip.30011",
        "text": "乐猿大圣！让我来看看你今天的心情！"
    },
    "30030012": {
        "deprecated": false,
        "Dev_Desc": "난모루지 102701",
        "id": 30030012,
        "alias": "Contents.Gossip.30012",
        "text": "我不知道。不要跟我说话。"
    },
    "30030013": {
        "deprecated": false,
        "Dev_Desc": "마을 주민 102702",
        "id": 30030013,
        "alias": "Contents.Gossip.30013",
        "text": "最近怎么看不到那个<textColor=[78:148:215:255]>大狗</>？"
    },
    "30030014": {
        "deprecated": false,
        "Dev_Desc": "마을 주민 102703",
        "id": 30030014,
        "alias": "Contents.Gossip.30014",
        "text": "<textColor=[78:148:215:255]>小希望</>？偶尔散步时还跟我打过招呼呢..."
    },
    "30030015": {
        "deprecated": false,
        "Dev_Desc": "마을 주민 102704",
        "id": 30030015,
        "alias": "Contents.Gossip.30015",
        "text": "<textColor=[78:148:215:255]>小希望</>更喜欢我。\n每次看到我都会跑过来！"
    },
    "30030016": {
        "deprecated": false,
        "Dev_Desc": "순찰 대원 102706",
        "id": 30030016,
        "alias": "Contents.Gossip.30016",
        "text": "我们无法守护一切。\n差不多就可以了。"
    },
    "30030017": {
        "deprecated": false,
        "Dev_Desc": "순찰 대원 102707",
        "id": 30030017,
        "alias": "Contents.Gossip.30017",
        "text": "虽然无法守护一切...\n尽全力吧。"
    },
    "30030018": {
        "deprecated": false,
        "Dev_Desc": "순찰대원 102709",
        "id": 30030018,
        "alias": "Contents.Gossip.30018",
        "text": "说真的...\n若还需要继续在这里巡逻的话，我宁愿离开搜阳。"
    },
    "30030019": {
        "deprecated": false,
        "Dev_Desc": "순찰대원 102710",
        "id": 30030019,
        "alias": "Contents.Gossip.30019",
        "text": "怎么会被安排到这种地方？"
    },
    "30030020": {
        "deprecated": false,
        "Dev_Desc": "이모작",
        "id": 30030020,
        "alias": "Contents.Gossip.30020",
        "text": "唉，农活差不多就可以。\n你们勤奋过头了。"
    },
    "30030021": {
        "deprecated": false,
        "Dev_Desc": "사모작",
        "id": 30030021,
        "alias": "Contents.Gossip.30021",
        "text": "能收割的时候，就该多收一点好呀。"
    },
    "30030022": {
        "deprecated": false,
        "Dev_Desc": "수비대원 102716",
        "id": 30030022,
        "alias": "Contents.Gossip.30022",
        "text": "大量的人与物资经过此地。\n因此更需要保障安全。"
    },
    "30030023": {
        "deprecated": false,
        "Dev_Desc": "수비대원 102717",
        "id": 30030023,
        "alias": "Contents.Gossip.30023",
        "text": "基地的守备就交给我们吧。"
    },
    "30030024": {
        "deprecated": false,
        "Dev_Desc": "오대야",
        "id": 30030024,
        "alias": "Contents.Gossip.30024",
        "text": "既然犯了罪，那就付出代价吧。"
    },
    "30030025": {
        "deprecated": false,
        "Dev_Desc": "수양대원 102726",
        "id": 30030025,
        "alias": "Contents.Gossip.30025",
        "text": "执勤中无异常！"
    },
    "30030026": {
        "deprecated": false,
        "Dev_Desc": "수양대원 102727",
        "id": 30030026,
        "alias": "Contents.Gossip.30026",
        "text": "找个僻静的角落休息一下吧..."
    },
    "30030027": {
        "deprecated": false,
        "Dev_Desc": "구구",
        "id": 30030027,
        "alias": "Contents.Gossip.30027",
        "text": "到底来不来呀喵！我很宽宏大量喵...我正在忍耐喵！"
    },
    "30030028": {
        "deprecated": false,
        "Dev_Desc": "거비나",
        "id": 30030028,
        "alias": "Contents.Gossip.30028",
        "text": "不知道什么时候又会蹦出来...小心一点吧。"
    },
    "30030029": {
        "deprecated": false,
        "Dev_Desc": "이름표 없는 대원",
        "id": 30030029,
        "alias": "Contents.Gossip.30029",
        "text": "为什么这么累？身体好沉重..."
