    },
    "40065057": {
        "deprecated": false,
        "Dev_Desc": "65004_6",
        "id": 40065057,
        "alias": "Contents.Quest.Guide.65058",
        "text": "（天哪，作者是\\n<textColor=[215:94:94:255]>中央警司</>...！）"
    },
    "40065058": {
        "deprecated": false,
        "Dev_Desc": "65007_1",
        "id": 40065058,
        "alias": "Contents.Quest.Guide.65059",
        "text": "(<textColor=[110:216:175:255]>你想探索更多的营地</>)"
    },
    "40065059": {
        "deprecated": false,
        "Dev_Desc": "65007_2_1_1",
        "id": 40065059,
        "alias": "Contents.Quest.Guide.65060",
