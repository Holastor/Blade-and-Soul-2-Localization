{
    "40075000": {
        "deprecated": false,
        "Dev_Desc": "[Lv.60] 탐사(검은새벽단 전...)",
        "id": 40075000,
        "alias": "Contents.Quest.Guide.75000",
        "text": "(여기에 <textColor=[110:216:175:255]>전초기지</>를 세울 정도로...\n<textColor=[215:94:94:255]>검은새벽단</>이 자리를 잡았구나)"
    },
    "40075001": {
        "deprecated": false,
        "Dev_Desc": "[Lv.60] 탐사(검은새벽단 전...)",
        "id": 40075001,
        "alias": "Contents.Quest.Guide.75001",
        "text": "(풍백문 대원들이 이렇게 되다니... <textColor=[215:94:94:255]>검은새벽단</>은 왜 이런 짓을?)"
    },
    "40075002": {
        "deprecated": false,
        "Dev_Desc": "[Lv.61] 탐사(가라앉은 묘지)",
        "id": 40075002,
        "alias": "Contents.Quest.Guide.75002",
        "text": "(왜 이곳의 <textColor=[215:94:94:255]>수와족</>은 적대적이지? \n조사가 필요해)"
    },
    "40075003": {
        "deprecated": false,
        "Dev_Desc": "[Lv.61] 탐사(가라앉은 묘지)",
        "id": 40075003,
        "alias": "Contents.Quest.Guide.75003",
        "text": "(결국 이들도 희생당한 존재겠지?)"
    },
    "40075004": {
        "deprecated": false,
        "Dev_Desc": "[Lv.61] 탐사(가라앉은 묘지)",
        "id": 40075004,
        "alias": "Contents.Quest.Guide.75004",
        "text": "(여기가 침수된 이후에\n이곳에 살게 된 걸까?)"
    },
    "40075005": {
        "deprecated": false,
        "Dev_Desc": "[Lv.61] 탐사(가라앉은 묘지)",
        "id": 40075005,
        "alias": "Contents.Quest.Guide.75005",
        "text": "(이곳의 <textColor=[215:94:94:255]>묘지기</>는 모두에게 적의를 보이는 걸까?)"
    },
    "40075006": {
        "deprecated": false,
        "Dev_Desc": "[Lv.61] 탐사(잿빛 하늘 숲)",
        "id": 40075006,
        "alias": "Contents.Quest.Guide.75006",
        "text": "(여기에도 <textColor=[215:94:94:255]>어둑시니</>가 있네. \n조사하고 지나가자)"
    },
    "40075007": {
        "deprecated": false,
        "Dev_Desc": "[Lv.61] 탐사(잿빛 하늘 숲)",
        "id": 40075007,
        "alias": "Contents.Quest.Guide.75007",
        "text": "(<textColor=[215:94:94:255]>침식된 어둑시니</>라... \n여기에도 탁회암의 영향이?)"
    },
    "40075008": {
        "deprecated": false,
        "Dev_Desc": "[Lv.61] 탐사(잿빛 하늘 숲)",
        "id": 40075008,
        "alias": "Contents.Quest.Guide.75008",
        "text": "(재 때문에 호흡도 불편한데... <textColor=[215:94:94:255]>녹각다귀</>에겐 상관없나?)"
    },
    "40075009": {
        "deprecated": false,
        "Dev_Desc": "[Lv.62] 탐사(야수의 소굴)",
        "id": 40075009,
        "alias": "Contents.Quest.Guide.75009",
        "text": "(여기엔 <textColor=[215:94:94:255]>변종 야수</>가? \n조사가 필요하겠어)"
    },
    "40075010": {
        "deprecated": false,
        "Dev_Desc": "[Lv.62] 탐사(야수의 소굴)",
        "id": 40075010,
        "alias": "Contents.Quest.Guide.75010",
        "text": "(이곳은 땅이 넓어 <textColor=[215:94:94:255]>야수</>들이 \n자리 잡기 좋겠어)"
    },
    "40075011": {
        "deprecated": false,
        "Dev_Desc": "[Lv.62] 탐사(야수의 소굴)",
        "id": 40075011,
        "alias": "Contents.Quest.Guide.75011",
        "text": "(<textColor=[215:94:94:255]>사슴</>들이 좋아하는 숲이 아니라 \n이런 곳에 모여 있다니...)"
    },
    "40075012": {
        "deprecated": false,
        "Dev_Desc": "[Lv.62] 탐사(포식자의 구릉지)",
        "id": 40075012,
        "alias": "Contents.Quest.Guide.75012",
        "text": "(괴상하게 생긴 <textColor=[215:94:94:255]>새</>다. \n좀 더 확인해 보자)"
    },
    "40075013": {
        "deprecated": false,
        "Dev_Desc": "[Lv.62] 탐사(포식자의 구릉지)",
        "id": 40075013,
        "alias": "Contents.Quest.Guide.75013",
        "text": "(<textColor=[215:94:94:255]>새</>와 <textColor=[215:94:94:255]>호랑이</>의 조합이라니... \n서로를 공격하진 않는 건가?)"
    },
    "40075014": {
        "deprecated": false,
        "Dev_Desc": "[Lv.63] 탐사(어둠 숲 북부)",
        "id": 40075014,
        "alias": "Contents.Quest.Guide.75014",
        "text": "(여기도 <textColor=[215:94:94:255]>외눈박이</>가 있네. \n한번 조사해 볼까?)"
    },
    "40075015": {
        "deprecated": false,
        "Dev_Desc": "[Lv.63] 탐사(어둠 숲 북부)",
        "id": 40075015,
        "alias": "Contents.Quest.Guide.75015",
        "text": "(한 눈으로도 살 수 있도록 \n적응해 온 걸까?)"
    },
    "40075016": {
        "deprecated": false,
        "Dev_Desc": "[Lv.63] 탐사(어둠 숲 북부)",
        "id": 40075016,
        "alias": "Contents.Quest.Guide.75016",
        "text": "(어둠 숲이라면 야금에게 \n최적의 환경일지 몰라)"
    },
    "40075017": {
        "deprecated": false,
        "Dev_Desc": "[Lv.63] 탐사(버림받은 자들...)",
        "id": 40075017,
        "alias": "Contents.Quest.Guide.75017",
        "text": "(이곳의 적들은 강하다. \n조사에 주의가 필요해)"
    },
    "40075018": {
        "deprecated": false,
        "Dev_Desc": "[Lv.63] 탐사(버림받은 자들...)",
        "id": 40075018,
        "alias": "Contents.Quest.Guide.75018",
        "text": "(<textColor=[215:94:94:255]>검은새벽단</>은 이런 짓을... \n절대 용서할 수 없어)"
    },
    "40075019": {
        "deprecated": false,
        "Dev_Desc": "[Lv.63] 탐사(버림받은 자들...)",
        "id": 40075019,
        "alias": "Contents.Quest.Guide.75019",
        "text": "(<textColor=[215:94:94:255]>녹각다귀</>는 예전부터 \n여기서 살았을까?)"
    },
    "40075020": {
        "deprecated": false,
        "Dev_Desc": "[Lv.63] 탐사(어둠 숲 남부)",
        "id": 40075020,
        "alias": "Contents.Quest.Guide.75020",
        "text": "(<textColor=[215:94:94:255]>맹독충</>? \n이들이 살고 있어 <textColor=[110:216:175:255]>맹독의 숲</>인가?)"
    },
    "40075021": {
        "deprecated": false,
        "Dev_Desc": "[Lv.63] 탐사(어둠 숲 남부)",
        "id": 40075021,
        "alias": "Contents.Quest.Guide.75021",
        "text": "(여기 환경이 <textColor=[215:94:94:255]>맹독충</>의 \n독을 만들었을 지도...)"
    },
    "40075022": {
        "deprecated": false,
        "Dev_Desc": "[Lv.64] 탐사(갈라진 대지)",
        "id": 40075022,
        "alias": "Contents.Quest.Guide.75022",
        "text": "(<textColor=[215:94:94:255]>불마자</>들이다... \n확인하고 가는 게 좋겠어)"
    },
    "40075023": {
        "deprecated": false,
        "Dev_Desc": "[Lv.64] 탐사(갈라진 대지)",
        "id": 40075023,
        "alias": "Contents.Quest.Guide.75023",
        "text": "(지속적인 균열로 인해 \n이곳의 환경은 너무 나빠졌어)"
    },
    "40075024": {
        "deprecated": false,
        "Dev_Desc": "[Lv.64] 탐사(갈라진 대지)",
        "id": 40075024,
        "alias": "Contents.Quest.Guide.75024",
        "text": "(<textColor=[215:94:94:255]>불마자</>가 끝도 없다. \n이들은 어디서 온 걸까?)"
    },
    "40075025": {
        "deprecated": false,
        "Dev_Desc": "[Lv.64] 탐사(불타는 대지)",
        "id": 40075025,
        "alias": "Contents.Quest.Guide.75025",
        "text": "(이렇게 넓게 <textColor=[215:94:94:255]>불마자</>가 \n자리 잡고 있다니...)"
    },
    "40075026": {
        "deprecated": false,
        "Dev_Desc": "[Lv.64] 탐사(불타는 대지)",
        "id": 40075026,
        "alias": "Contents.Quest.Guide.75026",
        "text": "(이 정도 크기의 균열은 처음 보는 것 같다. 피해 지역이 넓구나)"
    },
    "40075027": {
        "deprecated": false,
        "Dev_Desc": "[Lv.64] 탐사(검은새벽단 보...)",
        "id": 40075027,
        "alias": "Contents.Quest.Guide.75027",
        "text": "(<textColor=[215:94:94:255]>검은새벽단</>의 <textColor=[110:216:175:255]>보급소</>다. \n정보를 얻어 보자)"
    },
    "40075028": {
        "deprecated": false,
        "Dev_Desc": "[Lv.64] 탐사(검은새벽단 보...)",
        "id": 40075028,
        "alias": "Contents.Quest.Guide.75028",
        "text": "(<textColor=[215:94:94:255]>검은새벽단</>은 <textColor=[110:216:175:255]>보급소</>를 운영하면서 많은 인원을 관리하고 있다)"
    },
    "40075029": {
        "deprecated": false,
        "Dev_Desc": "[Lv.64] 탐사(검은새벽단 보...)",
        "id": 40075029,
        "alias": "Contents.Quest.Guide.75029",
        "text": "(이 <textColor=[215:94:94:255]>효조</>를 길들인 것도 정상적인 방법은 아니었을 거야)"
    },
    "40075030": {
        "deprecated": false,
        "Dev_Desc": "[Lv.65] 탐사(검은새벽단 수...)",
        "id": 40075030,
        "alias": "Contents.Quest.Guide.75030",
        "text": "(<textColor=[110:216:175:255]>(검은새벽단 수용소</>... 갑자기 적들이 강해졌다는 생각이 든다)"
    },
    "40075031": {
        "deprecated": false,
        "Dev_Desc": "[Lv.65] 탐사(검은새벽단 수...)",
        "id": 40075031,
        "alias": "Contents.Quest.Guide.75031",
        "text": "(여기저기서 사라졌던 사람들이 이곳으로 왔을까?)"
    },
    "40075032": {
        "deprecated": false,
        "Dev_Desc": "[Lv.65] 탐사(검은새벽단 수...)",
        "id": 40075032,
        "alias": "Contents.Quest.Guide.75032",
        "text": "(<textColor=[215:94:94:255]>야수</>들을 길들이기 위해 좋지 않은 방법을 썼겠지)"
    },
    "40075033": {
        "deprecated": false,
        "Dev_Desc": "[Lv.65] 탐사(검은새벽단 무...)",
        "id": 40075033,
        "alias": "Contents.Quest.Guide.75033",
        "text": "검은 새벽단 무기고"
    },
    "40075034": {
        "deprecated": false,
        "Dev_Desc": "[Lv.65] 탐사(검은새벽단 무...)",
        "id": 40075034,
        "alias": "Contents.Quest.Guide.75034",
        "text": "검은 새벽단 무기고"
    },
    "40075035": {
        "deprecated": false,
        "Dev_Desc": "[Lv.65] 탐사(오염된 분화구)",
        "id": 40075035,
        "alias": "Contents.Quest.Guide.75035",
        "text": "(뭔가 굉장한 기운이 느껴진다. \n이곳의 탐사는 조심해야 해)"
    },
    "40075036": {
        "deprecated": false,
        "Dev_Desc": "[Lv.65] 탐사(오염된 분화구)",
        "id": 40075036,
        "alias": "Contents.Quest.Guide.75036",
        "text": "(기름진 땅이라거나 살기 좋은 땅도 아니다. 뭔가 목적이 있을 거야)"
    },
    "40075037": {
        "deprecated": false,
        "Dev_Desc": "[Lv.65] 탐사(오염된 분화구)",
        "id": 40075037,
        "alias": "Contents.Quest.Guide.75037",
        "text": "(<textColor=[215:94:94:255]>포식귀</>까지 확인했다. \n<textColor=[110:216:175:255]>해룡산맥</> 지역 확인은 일단 끝났어)"
    },
    "40075038": {
        "deprecated": false,
        "Dev_Desc": "[Lv.61] 탐사(잿빛 하늘 숲)",
        "id": 40075038,
        "alias": "Contents.Quest.Guide.75038",
        "text": "(<textColor=[215:94:94:255]>곰</>과 <textColor=[215:94:94:255]>거미</>가 여기에 나와있다는 건 환경이 이상하다는 거겠지?)"
    },
    "40075039": {
        "deprecated": false,
        "Dev_Desc": "[Lv.61] 탐사(거인의 숲)",
        "id": 40075039,
        "alias": "Contents.Quest.Guide.75039",
        "text": "(<textColor=[215:94:94:255]>두억시니</>? 이곳의 적들은 강하다.\n탐사할 때 주의할 필요가 있겠어)"
    },
    "40075040": {
        "deprecated": false,
        "Dev_Desc": "[Lv.61] 탐사(거인의 숲)",
        "id": 40075040,
        "alias": "Contents.Quest.Guide.75040",
        "text": "(이렇게 큰 몸집이라니... \n먹이는 어떻게 충당하는 거지?)"
    },
    "40075041": {
        "deprecated": false,
        "Dev_Desc": "[Lv.61] 탐사(거인의 숲)",
        "id": 40075041,
        "alias": "Contents.Quest.Guide.75041",
        "text": "(<textColor=[215:94:94:255]>만다라화</>는 <textColor=[215:94:94:255]>두억시니</>와 같이 지내는데 문제가 없나 보다)"
    },
    "40075042": {
        "deprecated": false,
        "Dev_Desc": "[Lv.61] 탐사(거인의 숲)",
        "id": 40075042,
        "alias": "Contents.Quest.Guide.75042",
        "text": "(<textColor=[215:94:94:255]>묘면족</>은 어떻게 이 숲에서 \n살게 되었을까?)"
    },
    "40075043": {
        "deprecated": false,
        "Dev_Desc": "[Lv.62] 탐사(포식자의 구릉지)",
        "id": 40075043,
        "alias": "Contents.Quest.Guide.75043",
        "text": "(<textColor=[215:94:94:255]>포식귀</>? 뭐든 먹어 치우는 건가?)"
    },
    "40075044": {
        "deprecated": false,
        "Dev_Desc": "[Lv.63] 탐사(어둠 숲 남부)",
        "id": 40075044,
        "alias": "Contents.Quest.Guide.75044",
        "text": "(이 숲의 기운이 <textColor=[215:94:94:255]>수호령</>을 \n저렇게 만들었나?)"
    },
    "40075045": {
        "deprecated": false,
        "Dev_Desc": "[Lv.64] 탐사(불타는 대지)",
        "id": 40075045,
        "alias": "Contents.Quest.Guide.75045",
        "text": "(다양한 <textColor=[215:94:94:255]>불마자</>들이 있다. \n상대하기 쉽지 않겠어)"
    },
    "40075046": {
        "deprecated": false,
        "Dev_Desc": "[Lv.64] 탐사(불타는 대지)",
        "id": 40075046,
        "alias": "Contents.Quest.Guide.75046",
        "text": "(언젠가는 이곳은 다시 \n살아있는 땅으로 되돌려야 할 텐데...)"
    },
    "40075047": {
        "deprecated": false,
        "Dev_Desc": "[Lv.64] 탐사(어둠 숲 진입로)",
        "id": 40075047,
        "alias": "Contents.Quest.Guide.75047",
        "text": "(여기 <textColor=[215:94:94:255]>포식귀</>는 위험하게 생겼다. 확인하자)"
    },
    "40075048": {
        "deprecated": false,
        "Dev_Desc": "[Lv.64] 탐사(어둠 숲 진입로)",
        "id": 40075048,
        "alias": "Contents.Quest.Guide.75048",
        "text": "(<textColor=[215:94:94:255]>포식귀</>? 뭐든 먹어 치우는 건가?)"
    },
    "40075049": {
        "deprecated": false,
        "Dev_Desc": "[Lv.63] 탐사(어둠 숲 남부)",
        "id": 40075049,
        "alias": "Contents.Quest.Guide.75049",
        "text": "(<textColor=[215:94:94:255]>수호령</>이 왜 적대적인 행동을... \n조사해 보자)"
    },
    "40075050": {
        "deprecated": false,
        "Dev_Desc": "[Lv.64] 탐사(오염된 대지)",
        "id": 40075050,
        "alias": "Contents.Quest.Guide.75050",
        "text": "(여기 <textColor=[215:94:94:255]>담인초</>와 <textColor=[215:94:94:255]>투연초</>가 있다니... \n확인해 볼까?)"
    },
    "40075051": {
        "deprecated": false,
        "Dev_Desc": "[Lv.64] 탐사(오염된 대지)",
        "id": 40075051,
        "alias": "Contents.Quest.Guide.75051",
        "text": "(이런 척박한 환경에도 \n어떻게든 살아가고 있구나)"
    },
    "40075052": {
        "deprecated": false,
        "Dev_Desc": "[Lv.61] 탐사(잿빛 하늘 숲 북부)",
        "id": 40075052,
        "alias": "Contents.Quest.Guide.75052",
        "text": "(여기 <textColor=[215:94:94:255]>수목정령</>은 껍질을 보니 \n꽤나 오래 살아온 것 같아)"
    },
    "40075053": {
        "deprecated": false,
        "Dev_Desc": "[Lv.61] 탐사(잿빛 하늘 숲 남부)",
        "id": 40075053,
        "alias": "Contents.Quest.Guide.75053",
        "text": "(이곳에 <textColor=[215:94:94:255]>녹각다귀</>가 있네. \n조사해 보자)"
    }
}