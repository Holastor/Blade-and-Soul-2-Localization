{
    "40030000": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30000_1_pc 가이드",
        "id": 40030000,
        "alias": "Contents.Quest.Guide.30000",
        "text": "(일단 주변 사람들께\n물어보도록 하자)"
    },
    "40030001": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30000_2_pc 가이드",
        "id": 40030001,
        "alias": "Contents.Quest.Guide.30001",
        "text": "(그럼 <textColor=[110:216:175:255]>폭포</> 쪽으로 \n가보자!)"
    },
    "40030002": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30000_3_pc 가이드",
        "id": 40030002,
        "alias": "Contents.Quest.Guide.30002",
        "text": "(<textColor=[215:94:94:255]>만다라화</>를 처치하며\n주변을 살펴보자)"
    },
    "40030003": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30000_4_pc 가이드",
        "id": 40030003,
        "alias": "Contents.Quest.Guide.30003",
        "text": "(흔적을 찾을 수 없다.\n<textColor=[110:216:175:255]>폭포</> 근처를 찾아 볼까?)"
    },
    "40030004": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30000_5_pc 가이드",
        "id": 40030004,
        "alias": "Contents.Quest.Guide.30004",
        "text": "(저건? \n뭔가 공처럼 생겼다. \n자세히 보자)"
    },
    "40030005": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30000_6_pc 가이드",
        "id": 40030005,
        "alias": "Contents.Quest.Guide.30005",
        "text": "(<textColor=[78:148:215:255]>희망이</>란 말이...\n혹시 이 <textColor=[255:204:102:255]>방울</>이?)"
    },
    "40030006": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30000_6_pc 가이드",
        "id": 40030006,
        "alias": "Contents.Quest.Guide.30006",
        "text": "(<textColor=[78:148:215:255]>순찰대원</>에게 가서\n이야기해보자)"
    },
    "40030007": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30001_0_pc 가이드",
        "id": 40030007,
        "alias": "Contents.Quest.Guide.30007",
        "text": "(그 <textColor=[78:148:215:255]>동료분</>을 만나서 \n이야기를 들어보자!)"
    },
    "40030008": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30001_1_pc 가이드",
        "id": 40030008,
        "alias": "Contents.Quest.Guide.30008",
        "text": "(저기 있는 <textColor=[78:148:215:255]>대원들</>에게 \n물어보자!)"
    },
    "40030009": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30001_2_pc 가이드",
        "id": 40030009,
        "alias": "Contents.Quest.Guide.30009",
        "text": "(순찰 경로를 \n따라 가볼까?)"
    },
    "40030010": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30001_3_pc 가이드",
        "id": 40030010,
        "alias": "Contents.Quest.Guide.30010",
        "text": "(동물들이 \n이렇게나 많다니...)"
    },
    "40030011": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30001_4_pc 가이드",
        "id": 40030011,
        "alias": "Contents.Quest.Guide.30011",
        "text": "(몇 마리를 없앴는데\n별 차이가 없어 보인다)"
    },
    "40030012": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30001_5_pc 가이드",
        "id": 40030012,
        "alias": "Contents.Quest.Guide.30012",
        "text": "(이 정도라면 \n당분간은 순찰하지 \n않는 게 좋을지도...)"
    },
    "40030013": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30001_6_pc 가이드",
        "id": 40030013,
        "alias": "Contents.Quest.Guide.30013",
        "text": "(마지막 순찰 장소로 가서\n괴물이 있는지 찾아보자)"
    },
    "40030014": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30001_7_pc 가이드",
        "id": 40030014,
        "alias": "Contents.Quest.Guide.30014",
        "text": "(<textColor=[78:148:215:255]>대원들</>에게 가서\n상황을 알려 주자!)"
    },
    "40030015": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30002_0_pc 가이드",
        "id": 40030015,
        "alias": "Contents.Quest.Guide.30015",
        "text": "(전문가들에게 가보자!)"
    },
    "40030016": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30002_1_pc 가이드",
        "id": 40030016,
        "alias": "Contents.Quest.Guide.30016",
        "text": "(식량과 비료를 \n만드는 방법이 있겠구나)"
    },
    "40030017": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30002_1_pc 가이드",
        "id": 40030017,
        "alias": "Contents.Quest.Guide.30017",
        "text": "(결국은 \n몰아내는 수밖에 없구나)"
    },
    "40030018": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30002_1_pc 가이드",
        "id": 40030018,
        "alias": "Contents.Quest.Guide.30018",
        "text": "(내가 할 수 있는 일을\n하도록 하자!)"
    },
    "40030019": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30002_2_pc 가이드",
        "id": 40030019,
        "alias": "Contents.Quest.Guide.30019",
        "text": "(가서 주변을 살펴볼까?)"
    },
    "40030020": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30002_3_pc 가이드",
        "id": 40030020,
        "alias": "Contents.Quest.Guide.30020",
        "text": "(여기 분위기가 이상한데...\n주변을 좀 살펴볼까?)"
    },
    "40030021": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30002_4_pc 가이드",
        "id": 40030021,
        "alias": "Contents.Quest.Guide.30021",
        "text": "(이걸로 해결은 아니지만\n돌아가서 말을 전하자!)"
    },
    "40030022": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30003_0_pc 가이드",
        "id": 40030022,
        "alias": "Contents.Quest.Guide.30022",
        "text": "(내가 도와줘야겠다)"
    },
    "40030023": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30003_1_pc 가이드",
        "id": 40030023,
        "alias": "Contents.Quest.Guide.30023",
        "text": "(저런 걸 모아 달라고 \n했었지?)"
    },
    "40030024": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30003_2_pc 가이드",
        "id": 40030024,
        "alias": "Contents.Quest.Guide.30024",
        "text": "(생각보다 어렵지 않겠어)"
    },
    "40030025": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30003_2_pc 가이드",
        "id": 40030025,
        "alias": "Contents.Quest.Guide.30025",
        "text": "(저기 보이는 건 뭐지?\n뭔지 확인해 보자!)"
    },
    "40030026": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30003_3_pc 가이드",
        "id": 40030026,
        "alias": "Contents.Quest.Guide.30026",
        "text": "(이곳에 <textColor=[215:94:94:255]>창도단</>이?\n공격하려는 걸까?)"
    },
    "40030027": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30003_4_pc 가이드",
        "id": 40030027,
        "alias": "Contents.Quest.Guide.30027",
        "text": "(<textColor=[215:94:94:255]>창도단</>을 제거하는 데 \n성공했다)"
    },
    "40030028": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30003_4_pc 가이드",
        "id": 40030028,
        "alias": "Contents.Quest.Guide.30028",
        "text": "(<textColor=[255:204:102:255]>일지</> 같은 게 있다.\n계획이 적혀 있을지도...)"
    },
    "40030029": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30003_5_pc 가이드",
        "id": 40030029,
        "alias": "Contents.Quest.Guide.30029",
        "text": "(음... 이 정도만 하고\n<textColor=[255:204:102:255]>자재</>를 챙겨 돌아가자!)"
    },
    "40030030": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30003_6_pc 가이드",
        "id": 40030030,
        "alias": "Contents.Quest.Guide.30030",
        "text": "(이 정도면 충분하겠지?\n슬슬 돌아가자)"
    },
    "40030031": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30100_0_pc 가이드",
        "id": 40030031,
        "alias": "Contents.Quest.Guide.30031",
        "text": "(<textColor=[110:216:175:255]>뱀버섯 군락</> 쪽으로\n우선 가보자!)"
    },
    "40030032": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30100_1_pc 가이드",
        "id": 40030032,
        "alias": "Contents.Quest.Guide.30032",
        "text": "(약을 만들 수 있게\n<textColor=[215:94:94:255]>뱀버섯</>을 잡아보자!)"
    },
    "40030033": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30100_2_pc 가이드",
        "id": 40030033,
        "alias": "Contents.Quest.Guide.30033",
        "text": "(양손에 들고 갈 수 \n있을 만큼만 잡자)"
    },
    "40030034": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30100_3_pc 가이드",
        "id": 40030034,
        "alias": "Contents.Quest.Guide.30034",
        "text": "(버섯주와 전골 요리에 \n그런 의미가 있었구나)"
    },
    "40030035": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30101_0_pc 가이드",
        "id": 40030035,
        "alias": "Contents.Quest.Guide.30035",
        "text": "(진짜 혼령인지\n확인해 봐야겠다)"
    },
    "40030036": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30101_1_pc 가이드",
        "id": 40030036,
        "alias": "Contents.Quest.Guide.30036",
        "text": "(저기에 있는 게\n<textColor=[110:216:175:255]>묘지</>관리병인가?)"
    },
    "40030037": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30101_2_pc 가이드",
        "id": 40030037,
        "alias": "Contents.Quest.Guide.30037",
        "text": "(싫으셨나 보다.\n<textColor=[110:216:175:255]>묘지</> 안으로 가보자)"
    },
    "40030038": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30101_3_pc 가이드",
        "id": 40030038,
        "alias": "Contents.Quest.Guide.30038",
        "text": "(저기 누군가 있다.\n응? 혼령 같은데?)"
    },
    "40030039": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30101_4_pc 가이드",
        "id": 40030039,
        "alias": "Contents.Quest.Guide.30039",
        "text": "(이 정도면 내가\n도와드려야겠다)"
    },
    "40030040": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30101_5_pc 가이드",
        "id": 40030040,
        "alias": "Contents.Quest.Guide.30040",
        "text": "(정리된 것 같군. \n<textColor=[78:148:215:255]>관리병</>에게 가자)"
    },
    "40030041": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30101_6_pc 가이드",
        "id": 40030041,
        "alias": "Contents.Quest.Guide.30041",
        "text": "(임무를 다할 수 있도록\n그 혼령과 더 이상 마주치길 않길 기원해야지)"
    },
    "40030042": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30102_0_pc 가이드",
        "id": 40030042,
        "alias": "Contents.Quest.Guide.30042",
        "text": "(그래도 아는 사람인데\n필요하다면 도와야겠다)"
    },
    "40030043": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30102_1_비류하 가이드",
        "id": 40030043,
        "alias": "Contents.Quest.Guide.30043",
        "text": "마음에 드는 <textColor=[255:204:102:255]>꽃</>으로\n적당량 부탁드립니다"
    },
    "40030044": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30102_2_비류하 가이드",
        "id": 40030044,
        "alias": "Contents.Quest.Guide.30044",
        "text": "상태가 아주 좋은데요.\n그럼 절 따라오시죠"
    },
    "40030045": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30102_3_pc 가이드",
        "id": 40030045,
        "alias": "Contents.Quest.Guide.30045",
        "text": "(<textColor=[110:216:175:255]>묘지</>에 오는 거라서\n꽃을 챙긴 거였구나)"
    },
    "40030046": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30103_0_pc 가이드",
        "id": 40030046,
        "alias": "Contents.Quest.Guide.30046",
        "text": "(거점 위치만 \n알면 되는 건가?)"
    },
    "40030047": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30103_1_pc 가이드",
        "id": 40030047,
        "alias": "Contents.Quest.Guide.30047",
        "text": "(우선 주변 <textColor=[215:94:94:255]>혈비패</>들을\n처치하자!)"
    },
    "40030048": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30103_2_pc 가이드",
        "id": 40030048,
        "alias": "Contents.Quest.Guide.30048",
        "text": "(저기 안으로 한번\n들어가 볼까?)"
    },
    "40030049": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30103_3_pc 가이드",
        "id": 40030049,
        "alias": "Contents.Quest.Guide.30049",
        "text": "(여기가 기지 같은데?)"
    },
    "40030050": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30103_3_pc 가이드",
        "id": 40030050,
        "alias": "Contents.Quest.Guide.30050",
        "text": "(<textColor=[255:204:102:255]>증거물</>이 \n필요하다고 했지?)"
    },
    "40030051": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30103_4_pc 가이드",
        "id": 40030051,
        "alias": "Contents.Quest.Guide.30051",
        "text": "(이거면 증거가 될까?)"
    },
    "40030052": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30104_0_pc 가이드",
        "id": 40030052,
        "alias": "Contents.Quest.Guide.30052",
        "text": "(저러다 사고 나면\n찝찝하겠지? 가주자)"
    },
    "40030053": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30104_1_pc 가이드",
        "id": 40030053,
        "alias": "Contents.Quest.Guide.30053",
        "text": "(말도 없이 그냥 \n가버리는 군... \n따라 가자)"
    },
    "40030054": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30104_2_pc 가이드",
        "id": 40030054,
        "alias": "Contents.Quest.Guide.30054",
        "text": "(그냥 빨리 일을\n끝내버리자!)"
    },
    "40030055": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30104_3_영교원 구구 (공통)",
        "id": 40030055,
        "alias": "Contents.Quest.Guide.30055",
        "text": "자! 다음 장소로\n이동이다냥!"
    },
    "40030056": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30104_3_pc 가이드",
        "id": 40030056,
        "alias": "Contents.Quest.Guide.30056",
        "text": "(음... 위험할 것 같은데...\n주변을 좀 정리해보자)"
    },
    "40030057": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30104_4_영교원 구구 (공통)",
        "id": 40030057,
        "alias": "Contents.Quest.Guide.30057",
        "text": "자, 여기 <textColor=[255:204:102:255]>흙</>도\n채집해라냥!"
    },
    "40030058": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30104_4_pc 가이드",
        "id": 40030058,
        "alias": "Contents.Quest.Guide.30058",
        "text": "(남의 말은 거의\n듣지 않는 것 같다)"
    },
    "40030059": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30104_5_pc 가이드",
        "id": 40030059,
        "alias": "Contents.Quest.Guide.30059",
        "text": "(채집은 완료했다.\n말을 걸어 볼까?)"
    },
    "40030060": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30104_6_pc 가이드",
        "id": 40030060,
        "alias": "Contents.Quest.Guide.30060",
        "text": "(불쾌한 건 사실이다.\n없애도록 하자)"
    },
    "40030061": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30104_7_영교원 구구 (공통)",
        "id": 40030061,
        "alias": "Contents.Quest.Guide.30061",
        "text": "수고했다냥!\n보수를 주겠다냥."
    },
    "40030062": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30104_7_pc 가이드",
        "id": 40030062,
        "alias": "Contents.Quest.Guide.30062",
        "text": "(결국 일을 모두\n끝내버렸네)"
    },
    "40030063": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30104_8_pc 가이드",
        "id": 40030063,
        "alias": "Contents.Quest.Guide.30063",
        "text": "(나름 재밌긴 했지만\n이 정도로 끝내자)"
    },
    "40030064": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30105_0_pc 가이드",
        "id": 40030064,
        "alias": "Contents.Quest.Guide.30064",
        "text": "(여기 <textColor=[215:94:94:255]>인면충</>도 많은데\n저러다 다칠지도...)"
    },
    "40030065": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30105_1_영교원 구구 (공통)",
        "id": 40030065,
        "alias": "Contents.Quest.Guide.30065",
        "text": "일단 저기 불쾌한\n녀석들부터 없애라냥!"
    },
    "40030066": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30105_2_영교원 구구 (공통)",
        "id": 40030066,
        "alias": "Contents.Quest.Guide.30066",
        "text": "이리 와서\n내 이야기를 들어라냥."
    },
    "40030067": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30105_3_pc 가이드",
        "id": 40030067,
        "alias": "Contents.Quest.Guide.30067",
        "text": "(그렇게 생각할 수도 있군)"
    },
    "40030068": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30200_1_pc 가이드",
        "id": 40030068,
        "alias": "Contents.Quest.Guide.30068",
        "text": "(안전 난간 설치는\n좋은 생각인 것 같다)"
    },
    "40030069": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30200_2_pc 가이드",
        "id": 40030069,
        "alias": "Contents.Quest.Guide.30069",
        "text": "(<textColor=[255:204:102:255]>나무</>는 충분히 구했다)"
    },
    "40030070": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30200_2_pc 가이드",
        "id": 40030070,
        "alias": "Contents.Quest.Guide.30070",
        "text": "(이 정도면 충분히\n바를 수 있겠어)"
    },
    "40030071": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30200_2_pc 가이드",
        "id": 40030071,
        "alias": "Contents.Quest.Guide.30071",
        "text": "(이제 돌아가 볼까?)"
    },
    "40030072": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30201_0_pc 가이드",
        "id": 40030072,
        "alias": "Contents.Quest.Guide.30072",
        "text": "(꽤 위험한 일로 보인다)"
    },
    "40030073": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30201_1_pc 가이드",
        "id": 40030073,
        "alias": "Contents.Quest.Guide.30073",
        "text": "(관측 장소로\n이동해 보자)"
    },
    "40030074": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30201_2_pc 가이드",
        "id": 40030074,
        "alias": "Contents.Quest.Guide.30074",
        "text": "(이곳인가?)"
    },
    "40030075": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30201_3_pc 가이드",
        "id": 40030075,
        "alias": "Contents.Quest.Guide.30075",
        "text": "(더 안쪽으로\n들어가 보자)"
    },
    "40030076": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30201_4_pc 가이드",
        "id": 40030076,
        "alias": "Contents.Quest.Guide.30076",
        "text": "(큰일이다. \n<textColor=[215:94:94:255]>낙원족</>이 공격해온다!)"
    },
    "40030077": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30201_4_pc 가이드",
        "id": 40030077,
        "alias": "Contents.Quest.Guide.30077",
        "text": "(일단 <textColor=[215:94:94:255]>낙원족</>을\n몰아 내보자!)"
    },
    "40030078": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30201_5_pc 가이드",
        "id": 40030078,
        "alias": "Contents.Quest.Guide.30078",
        "text": "(다행이다.\n일단 안정된 것 같다)"
    },
    "40030079": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30201_6_pc 가이드",
        "id": 40030079,
        "alias": "Contents.Quest.Guide.30079",
        "text": "(<textColor=[215:94:94:255]>낙원대성</>을\n화나게 해선 안 돼)"
    },
    "40030080": {
        "deprecated": false,
        "Dev_Desc": "태록림 서브 캐릭터 이름",
        "id": 40030080,
        "alias": "Contents.Quest.Guide.30080",
        "text": "풍백문 장로 비류하"
    },
    "40030081": {
        "deprecated": false,
        "Dev_Desc": "태록림 서브 캐릭터 이름",
        "id": 40030081,
        "alias": "Contents.Quest.Guide.30081",
        "text": "영교원 구구"
    },
    "40030082": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30004_0_pc 가이드",
        "id": 40030082,
        "alias": "Contents.Quest.Guide.30082",
        "text": "(자신이 가보겠다고 한다)"
    },
    "40030083": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30004_1_pc 가이드",
        "id": 40030083,
        "alias": "Contents.Quest.Guide.30083",
        "text": "(정말 많긴 하구나)"
    },
    "40030084": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30004_2_pc 가이드_1",
        "id": 40030084,
        "alias": "Contents.Quest.Guide.30084",
        "text": "(나름 없애긴 했는데...)"
    },
    "40030085": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30004_2_pc 가이드_2",
        "id": 40030085,
        "alias": "Contents.Quest.Guide.30085",
        "text": "(혼자서 처리할 양은\n아닌 것 같다)"
    },
    "40030086": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30005_0_pc 가이드",
        "id": 40030086,
        "alias": "Contents.Quest.Guide.30086",
        "text": "(<textColor=[215:94:94:255]>어둑시니</>가 얼마나\n많은지 확인해보자)"
    },
    "40030087": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30005_1_pc 가이드",
        "id": 40030087,
        "alias": "Contents.Quest.Guide.30087",
        "text": "(이 정도라면...\n해변 산책도 못 하겠군)"
    },
    "40030088": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30005_2_pc 가이드_1",
        "id": 40030088,
        "alias": "Contents.Quest.Guide.30088",
        "text": "(<textColor=[215:94:94:255]>어둑시니</>가 어디서\n나오는 걸까?)"
    },
    "40030089": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30005_2_pc 가이드_2",
        "id": 40030089,
        "alias": "Contents.Quest.Guide.30089",
        "text": "(잡아도 끝이 없다)"
    },
    "40030090": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30106_0_pc 가이드",
        "id": 40030090,
        "alias": "Contents.Quest.Guide.30090",
        "text": "(일단 가는 길을\n정리해야겠군)"
    },
    "40030091": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30106_1_pc 가이드",
        "id": 40030091,
        "alias": "Contents.Quest.Guide.30091",
        "text": "(이제 <textColor=[110:216:175:255]>늪지</>로 이동하자)"
    },
    "40030092": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30106_2_pc 가이드",
        "id": 40030092,
        "alias": "Contents.Quest.Guide.30092",
        "text": "(대략 절반은 온 것 같다)"
    },
    "40030093": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30106_3_pc 가이드",
        "id": 40030093,
        "alias": "Contents.Quest.Guide.30093",
        "text": "(여기도 길이 험난하구나)"
    },
    "40030094": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30106_4_pc 가이드",
        "id": 40030094,
        "alias": "Contents.Quest.Guide.30094",
        "text": "(<textColor=[110:216:175:255]>안개 숲 개척지</>로 가자)"
    },
    "40030095": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30106_5_pc 가이드_1",
        "id": 40030095,
        "alias": "Contents.Quest.Guide.30095",
        "text": "(여기까진\n잘 돌아왔는데...)"
    },
    "40030096": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30106_5_pc 가이드_2",
        "id": 40030096,
        "alias": "Contents.Quest.Guide.30096",
        "text": "(<textColor=[110:216:175:255]>비경의 자드락</>까지\n과연 갈 수 있을까?)"
    },
    "40030097": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30106_6_pc 가이드_1",
        "id": 40030097,
        "alias": "Contents.Quest.Guide.30097",
        "text": "(혹시 이상한데 \n떨어지면 길을 잃겠지)"
    },
    "40030098": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30106_6_pc 가이드_2",
        "id": 40030098,
        "alias": "Contents.Quest.Guide.30098",
        "text": "(따라가 주도록 하자)"
    },
    "40030099": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30202_0_pc 가이드",
        "id": 40030099,
        "alias": "Contents.Quest.Guide.30099",
        "text": "(전투 살충?\n그냥 처치하면 되겠지?)"
    },
    "40030100": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30202_1_pc 가이드",
        "id": 40030100,
        "alias": "Contents.Quest.Guide.30100",
        "text": "(다음은 <textColor=[110:216:175:255]>언덕</>쪽인가?)"
    },
    "40030101": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30202_2_pc 가이드",
        "id": 40030101,
        "alias": "Contents.Quest.Guide.30101",
        "text": "(정기 작업이라니...\n짜증 날 수도 있겠다)"
    },
    "40030102": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30203_0_pc 가이드",
        "id": 40030102,
        "alias": "Contents.Quest.Guide.30102",
        "text": "(조심해야겠다)"
    },
    "40030103": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30203_1_pc 가이드",
        "id": 40030103,
        "alias": "Contents.Quest.Guide.30103",
        "text": "(일단 이곳에 거대 괴물은 \n없는 것 같다) "
    },
    "40030104": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30204_0_pc 가이드",
        "id": 40030104,
        "alias": "Contents.Quest.Guide.30104",
        "text": "(<textColor=[110:216:175:255]>비탈길</>로 가보자)"
    },
    "40030105": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30204_1_pc 가이드",
        "id": 40030105,
        "alias": "Contents.Quest.Guide.30105",
        "text": "(이 정도면 충분히\n많은 거겠지?)"
    },
    "40030106": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30204_2_pc 가이드",
        "id": 40030106,
        "alias": "Contents.Quest.Guide.30106",
        "text": "(얻으면 된다는 말이\n그런 의미였던 걸까?)"
    },
    "40030107": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30107_0_pc 가이드",
        "id": 40030107,
        "alias": "Contents.Quest.Guide.30107",
        "text": "(확실히 신경 쓰일 법하다)"
    },
    "40030108": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30107_1_pc 가이드",
        "id": 40030108,
        "alias": "Contents.Quest.Guide.30108",
        "text": "(수가 좀 줄었을까?\n도움이 되면 좋겠다)"
    },
    "40030109": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30003_6_pc 가이드",
        "id": 40030109,
        "alias": "Contents.Quest.Guide.30109",
        "text": "(아직 부족해.\n좀 더 찾아보자)"
    },
    "40030110": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30003_7_pc 가이드",
        "id": 40030110,
        "alias": "Contents.Quest.Guide.30110",
        "text": "(하나만 더 가져갈까?)"
    },
    "40030111": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30102_2_pc 가이드",
        "id": 40030111,
        "alias": "Contents.Quest.Guide.30111",
        "text": "(얼마나 따야 할까?)"
    },
    "40030112": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30102_3_pc 가이드",
        "id": 40030112,
        "alias": "Contents.Quest.Guide.30112",
        "text": "(음... 한 송이만 더 따 볼까?)"
    },
    "40030113": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30200_2-1_pc 가이드",
        "id": 40030113,
        "alias": "Contents.Quest.Guide.30113",
        "text": "(나무는 넉넉하게 챙겨야겠지)"
    },
    "40030114": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30200_2-2_pc 가이드",
        "id": 40030114,
        "alias": "Contents.Quest.Guide.30114",
        "text": "(모자란 것보단 남는 게 좋을 거야)"
    },
    "40030115": {
        "deprecated": false,
        "Dev_Desc": "태록림서브_30101_3_pc 가이드",
        "id": 40030115,
        "alias": "Contents.Quest.Guide.30115",
        "text": "(자연스럽게 \n말을 걸어보자)"
    },
    "40030116": {
        "deprecated": false,
        "Dev_Desc": "30006",
        "id": 40030116,
        "alias": "Contents.Quest.Guide.30116",
        "text": "<textColor=[110:216:175:255]>혈비의 숲</>에서 뵙겠습니다. \n소협."
    },
    "40030117": {
        "deprecated": false,
        "Dev_Desc": "30006",
        "id": 40030117,
        "alias": "Contents.Quest.Guide.30117",
        "text": "우선 <textColor=[110:216:175:255]>혈비패 기지</>를 \n살펴봐야 할 것 같습니다."
    },
    "40030118": {
        "deprecated": false,
        "Dev_Desc": "30006",
        "id": 40030118,
        "alias": "Contents.Quest.Guide.30118",
        "text": "가는 길에 <textColor=[215:94:94:255]>혈비패</>의 습격을\n받지 않게 조심하십시오!"
    },
    "40030119": {
        "deprecated": false,
        "Dev_Desc": "30006",
        "id": 40030119,
        "alias": "Contents.Quest.Guide.30119",
        "text": "(<textColor=[110:216:175:255]>기지</>를 탐색해보자)"
    },
    "40030120": {
        "deprecated": false,
        "Dev_Desc": "30006",
        "id": 40030120,
        "alias": "Contents.Quest.Guide.30120",
        "text": "이제 그들의 <textColor=[255:204:102:255]>습격 계획서</>를\n손에 넣어야 합니다."
    },
    "40030121": {
        "deprecated": false,
        "Dev_Desc": "30006",
        "id": 40030121,
        "alias": "Contents.Quest.Guide.30121",
        "text": "(<textColor=[255:204:102:255]>습격 계획서</>를 얻었다)"
    },
    "40030122": {
        "deprecated": false,
        "Dev_Desc": "30006",
        "id": 40030122,
        "alias": "Contents.Quest.Guide.30122",
        "text": "표시된 위치에 <textColor=[255:204:102:255]>함정</>을\n설치해주시면 될 것 같습니다."
    },
    "40030123": {
        "deprecated": false,
        "Dev_Desc": "30006",
        "id": 40030123,
        "alias": "Contents.Quest.Guide.30123",
        "text": "(설치가 끝났다. \n<textColor=[78:148:215:255]>홍매화</>에게 돌아가자)"
    },
    "40030124": {
        "deprecated": false,
        "Dev_Desc": "30108",
        "id": 40030124,
        "alias": "Contents.Quest.Guide.30124",
        "text": "(<textColor=[110:216:175:255]>안개 그늘 숲</>으로 가자)"
    },
    "40030125": {
        "deprecated": false,
        "Dev_Desc": "30108",
        "id": 40030125,
        "alias": "Contents.Quest.Guide.30125",
        "text": "역시 자네 실력은\n알아줘야 한다니까."
    },
    "40030126": {
        "deprecated": false,
        "Dev_Desc": "30108",
        "id": 40030126,
        "alias": "Contents.Quest.Guide.30126",
        "text": "부탁한 김에 늪지에 있는 \n<textColor=[215:94:94:255]>매화단 단장의 환영</>도 \n좀 부탁하네."
    },
    "40030127": {
        "deprecated": false,
        "Dev_Desc": "30108",
        "id": 40030127,
        "alias": "Contents.Quest.Guide.30127",
        "text": "역시 자네야.\n<textColor=[110:216:175:255]>개척지</>에서 봄세."
    },
    "40030128": {
        "deprecated": false,
        "Dev_Desc": "30108",
        "id": 40030128,
        "alias": "Contents.Quest.Guide.30128",
        "text": "(<textColor=[110:216:175:255]>개척지</>로 가자)"
    },
    "40030129": {
        "deprecated": false,
        "Dev_Desc": "30109",
        "id": 40030129,
        "alias": "Contents.Quest.Guide.30129",
        "text": "우선 <textColor=[215:94:94:255]>뱀버섯</>들이\n많은 곳을 부탁한다냥!"
    },
    "40030130": {
        "deprecated": false,
        "Dev_Desc": "30109",
        "id": 40030130,
        "alias": "Contents.Quest.Guide.30130",
        "text": "이번엔 \n<textColor=[110:216:175:255]>개척자 위령묘</>다냥!"
    },
    "40030131": {
        "deprecated": false,
        "Dev_Desc": "30109",
        "id": 40030131,
        "alias": "Contents.Quest.Guide.30131",
        "text": "앗?! <textColor=[110:216:175:255]>풍요 우림 거점</>에서\n만났던 인간이다냥!"
    },
    "40030132": {
        "deprecated": false,
        "Dev_Desc": "30109",
        "id": 40030132,
        "alias": "Contents.Quest.Guide.30132",
        "text": "(낯이 익은 것 같다)"
    },
    "40030133": {
        "deprecated": false,
        "Dev_Desc": "30109",
        "id": 40030133,
        "alias": "Contents.Quest.Guide.30133",
        "text": "잠깐 나 좀 보자냥."
    },
    "40030134": {
        "deprecated": false,
        "Dev_Desc": "30109",
        "id": 40030134,
        "alias": "Contents.Quest.Guide.30134",
        "text": "(바롱에게 돌아가자)"
    },
    "40030135": {
        "deprecated": false,
        "Dev_Desc": "30109",
        "id": 40030135,
        "alias": "Contents.Quest.Guide.30135",
        "text": "(휴... 진땀뺐다)"
    },
    "40030136": {
        "deprecated": false,
        "Dev_Desc": "30110",
        "id": 40030136,
        "alias": "Contents.Quest.Guide.30136",
        "text": "<textColor=[255:204:102:255]>밀렵품</>의 유통도\n막아야 하오."
    },
    "40030137": {
        "deprecated": false,
        "Dev_Desc": "30110",
        "id": 40030137,
        "alias": "Contents.Quest.Guide.30137",
        "text": "그들의 <textColor=[255:204:102:255]>밀렵품</>을\n회수해주시오."
    },
    "40030138": {
        "deprecated": false,
        "Dev_Desc": "30110",
        "id": 40030138,
        "alias": "Contents.Quest.Guide.30138",
        "text": "(돌아가는 길을\n확보해야겠어)"
    },
    "40030139": {
        "deprecated": false,
        "Dev_Desc": "30110",
        "id": 40030139,
        "alias": "Contents.Quest.Guide.30139",
        "text": "(<textColor=[78:148:215:255]>잿빛바람</>에게 돌아가자)"
    },
    "40030140": {
        "deprecated": false,
        "Dev_Desc": "30110",
        "id": 40030140,
        "alias": "Contents.Quest.Guide.30140",
        "text": "(밀렵은 언제쯤\n완벽하게 근절될까...?)"
    },
    "40030141": {
        "deprecated": false,
        "Dev_Desc": "30205",
        "id": 40030141,
        "alias": "Contents.Quest.Guide.30141",
        "text": "(<textColor=[110:216:175:255]>낙원 구릉</>으로 가보자)"
    },
    "40030142": {
        "deprecated": false,
        "Dev_Desc": "30205",
        "id": 40030142,
        "alias": "Contents.Quest.Guide.30142",
        "text": "오, 고마워."
    },
    "40030143": {
        "deprecated": false,
        "Dev_Desc": "30205",
        "id": 40030143,
        "alias": "Contents.Quest.Guide.30143",
        "text": "뭐야, <textColor=[78:148:215:255]>강철주먹</>.\n직접 하라고 했더니."
    },
    "40030144": {
        "deprecated": false,
        "Dev_Desc": "30205",
        "id": 40030144,
        "alias": "Contents.Quest.Guide.30144",
        "text": "<textColor=[78:148:215:255]>달, 달빛바람</>?!\n여, 여긴 어쩐 일이야."
    },
    "40030145": {
        "deprecated": false,
        "Dev_Desc": "30205",
        "id": 40030145,
        "alias": "Contents.Quest.Guide.30145",
        "text": "역시 믿음직스러워.\n누구랑은 다르게..."
    },
    "40030146": {
        "deprecated": false,
        "Dev_Desc": "30205",
        "id": 40030146,
        "alias": "Contents.Quest.Guide.30146",
        "text": "그 누구가 나는 아니지...?"
    },
    "40030147": {
        "deprecated": false,
        "Dev_Desc": "30205",
        "id": 40030147,
        "alias": "Contents.Quest.Guide.30147",
        "text": "조용히 해.\n아 참, 하나만 더 부탁할게."
    },
    "40030148": {
        "deprecated": false,
        "Dev_Desc": "30205",
        "id": 40030148,
        "alias": "Contents.Quest.Guide.30148",
        "text": "<textColor=[255:204:102:255]>검은 진흙</>을 좀 가져다줘.\n<textColor=[78:148:215:255]>곽호룡 상전주</>가\n특별히 추천하더라고."
    },
    "40030149": {
        "deprecated": false,
        "Dev_Desc": "30205",
        "id": 40030149,
        "alias": "Contents.Quest.Guide.30149",
        "text": "품질 좋은 흙이네.\n자, 여기 보상이야."
    },
    "40030150": {
        "deprecated": false,
        "Dev_Desc": "30205",
        "id": 40030150,
        "alias": "Contents.Quest.Guide.30150",
        "text": "그럼 또 봐."
    },
    "40030151": {
        "deprecated": false,
        "Dev_Desc": "30206",
        "id": 40030151,
        "alias": "Contents.Quest.Guide.30151",
        "text": "(알겠다고 한다)"
    },
    "40030152": {
        "deprecated": false,
        "Dev_Desc": "30206",
        "id": 40030152,
        "alias": "Contents.Quest.Guide.30152",
        "text": "흠... <textColor=[215:94:94:255]>묘면족</>이\n생각보다 많군."
    },
    "40030153": {
        "deprecated": false,
        "Dev_Desc": "30206",
        "id": 40030153,
        "alias": "Contents.Quest.Guide.30153",
        "text": "통행로에서 그들을\n조금만 밀어내주게."
    },
    "40030154": {
        "deprecated": false,
        "Dev_Desc": "30206",
        "id": 40030154,
        "alias": "Contents.Quest.Guide.30154",
        "text": "그 정도면 될 것 같네.\n역시 자네야."
    },
    "40030155": {
        "deprecated": false,
        "Dev_Desc": "30206",
        "id": 40030155,
        "alias": "Contents.Quest.Guide.30155",
        "text": "그럼 <textColor=[110:216:175:255]>쉼터</>에서\n기다리고 있겠네."
    },
    "40030156": {
        "deprecated": false,
        "Dev_Desc": "30206",
        "id": 40030156,
        "alias": "Contents.Quest.Guide.30156",
        "text": "(<textColor=[110:216:175:255]>풀내음 언덕길</>로 가자)"
    },
    "40030157": {
        "deprecated": false,
        "Dev_Desc": "30206",
        "id": 40030157,
        "alias": "Contents.Quest.Guide.30157",
        "text": "아무래도 자네가\n잎을 구해줘야겠네."
    },
    "40030158": {
        "deprecated": false,
        "Dev_Desc": "30206",
        "id": 40030158,
        "alias": "Contents.Quest.Guide.30158",
        "text": "일꾼들이 워낙 겁을\n집어삼켜서 말이야.\n부탁하겠네."
    },
    "40030159": {
        "deprecated": false,
        "Dev_Desc": "30206",
        "id": 40030159,
        "alias": "Contents.Quest.Guide.30159",
        "text": "덕분에 행사 준비를\n잘 마쳤네."
    },
    "40030160": {
        "deprecated": false,
        "Dev_Desc": "30206",
        "id": 40030160,
        "alias": "Contents.Quest.Guide.30160",
        "text": "약소하지만 준비한\n보상이네."
    },
    "40030161": {
        "deprecated": false,
        "Dev_Desc": "30206",
        "id": 40030161,
        "alias": "Contents.Quest.Guide.30161",
        "text": "(감사 인사를 건넨다)"
    },
    "40030162": {
        "deprecated": false,
        "Dev_Desc": "30207",
        "id": 40030162,
        "alias": "Contents.Quest.Guide.30162",
        "text": "감사합니다. 소협.\n큰 힘이 됐습니다."
    },
    "40030163": {
        "deprecated": false,
        "Dev_Desc": "30207",
        "id": 40030163,
        "alias": "Contents.Quest.Guide.30163",
        "text": "<textColor=[110:216:175:255]>폭포 남부 숲</>으로\n가보시면 반가운\n얼굴을 보실 겁니다."
    },
    "40030164": {
        "deprecated": false,
        "Dev_Desc": "30207",
        "id": 40030164,
        "alias": "Contents.Quest.Guide.30164",
        "text": "이쪽입니다. 소협."
    },
    "40030165": {
        "deprecated": false,
        "Dev_Desc": "30207",
        "id": 40030165,
        "alias": "Contents.Quest.Guide.30165",
        "text": "그들에게 평안함이 있기를...\n감사합니다. 소협"
    },
    "40030166": {
        "deprecated": false,
        "Dev_Desc": "캐릭터 이름",
        "id": 40030166,
        "alias": "Contents.Quest.Guide.30166",
        "text": "매화단 단장 홍매화"
    },
    "40030167": {
        "deprecated": false,
        "Dev_Desc": "캐릭터 이름",
        "id": 40030167,
        "alias": "Contents.Quest.Guide.30167",
        "text": "도화상전 상전주 곽호룡"
    },
    "40030168": {
        "deprecated": false,
        "Dev_Desc": "캐릭터 이름",
        "id": 40030168,
        "alias": "Contents.Quest.Guide.30168",
        "text": "영교원 수련원 무무"
    },
    "40030169": {
        "deprecated": false,
        "Dev_Desc": "캐릭터 이름",
        "id": 40030169,
        "alias": "Contents.Quest.Guide.30169",
        "text": "영교원 수련원 밍밍"
    },
    "40030170": {
        "deprecated": false,
        "Dev_Desc": "캐릭터 이름",
        "id": 40030170,
        "alias": "Contents.Quest.Guide.30170",
        "text": "야생 보전회 회장 잿빛바람"
    },
    "40030171": {
        "deprecated": false,
        "Dev_Desc": "캐릭터 이름",
        "id": 40030171,
        "alias": "Contents.Quest.Guide.30171",
        "text": "금경철문 강철주먹"
    },
    "40030172": {
        "deprecated": false,
        "Dev_Desc": "캐릭터 이름",
        "id": 40030172,
        "alias": "Contents.Quest.Guide.30172",
        "text": "금경철문 문주 달빛바람"
    },
    "40030173": {
        "deprecated": false,
        "Dev_Desc": "캐릭터 이름",
        "id": 40030173,
        "alias": "Contents.Quest.Guide.30173",
        "text": "에레다 부문주 오름"
    },
    "40030174": {
        "deprecated": false,
        "Dev_Desc": "캐릭터 이름",
        "id": 40030174,
        "alias": "Contents.Quest.Guide.30174",
        "text": "???"
    },
    "40030175": {
        "deprecated": false,
        "Dev_Desc": "캐릭터 이름",
        "id": 40030175,
        "alias": "Contents.Quest.Guide.30175",
        "text": "에레다 정예단원 도하"
    },
    "40030176": {
        "deprecated": false,
        "Dev_Desc": "30208_수락",
        "id": 40030176,
        "alias": "Contents.Quest.Guide.30176",
        "text": "(나를 부르는 것 같은데…\n다른 바롱과는 사뭇 달라보인다)"
    },
    "40030177": {
        "deprecated": false,
        "Dev_Desc": "30208_2",
        "id": 40030177,
        "alias": "Contents.Quest.Guide.30177",
        "text": "(바롱이 벌써 사라졌네.\n묘하게 수상하단 말이야…)"
    },
    "40030178": {
        "deprecated": false,
        "Dev_Desc": "30208_2",
        "id": 40030178,
        "alias": "Contents.Quest.Guide.30178",
        "text": "(앞으로 언령석을 얻으면\n야루의 상자를 꼭 이용하자)"
    }
}