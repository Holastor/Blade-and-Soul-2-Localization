    "400010042": {
        "deprecated": false,
        "Dev_Desc": "반달서브_10013_1_완료_PC",
        "id": 400010042,
        "alias": "Contents.Quest.Guide.10042",
        "text": "(사연이 있어 보이는\n<textColor=[78:148:215:255]>나침반</>을 발견했다)"
