    "400010044": {
        "deprecated": false,
        "Dev_Desc": "반달서브_10014_0_완료_PC",
        "id": 400010044,
        "alias": "Contents.Quest.Guide.10044",
        "text": "(으아아... \n<textColor=[78:148:215:255]>이상한 사람</>이 날 부른다)"
