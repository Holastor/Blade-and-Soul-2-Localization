    "400010043": {
        "deprecated": false,
        "Dev_Desc": "반달서브_10013_2_완료_PC",
        "id": 400010043,
        "alias": "Contents.Quest.Guide.10043",
        "text": "(<textColor=[78:148:215:255]>잊혀진 탐사대</>라...\n흥미로운 이야기다)"
