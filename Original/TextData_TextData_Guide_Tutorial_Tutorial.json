    },
    "40000512": {
        "deprecated": false,
        "Dev_Desc": "자동 사용 튜토리얼",
        "id": 40000512,
        "alias": "Contents.Tutorial.Guide.512",
        "text": "(<textColor=[255:204:102:255]>왕만두</>를 먹었으니\n더 <textColor=[255:204:102:255]>강하게 공격</>할 수 있겠어.\n자, 몸을 풀어볼까?)"
    },
    "40000517": {
        "deprecated": false,
        "Dev_Desc": "이동 튜토리얼",
        "id": 40000517,
        "alias": "Contents.Tutorial.Guide.517",
