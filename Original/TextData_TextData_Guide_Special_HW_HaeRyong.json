    },
    "40065051": {
        "deprecated": false,
        "Dev_Desc": "견주리_검은새벽단 수용소",
        "id": 40065051,
        "alias": "Contents.Quest.Guide.65051",
        "text": "견주리"
    },
    "40065052": {
        "deprecated": false,
        "Dev_Desc": "견주리_합동초소",
        "id": 40065052,
        "alias": "Contents.Quest.Guide.65052",
        "text": "견주리"
    },
    "40065054": {
        "deprecated": false,
        "Dev_Desc": "65000_5",
        "id": 40065054,
        "alias": "Contents.Quest.Guide.65054",
        "text": "(왜 또 소란스러운 소리가\n들리는 거지?)"
    },
    "40065055": {
        "deprecated": false,
        "Dev_Desc": "65004_5",
        "id": 40065055,
        "alias": "Contents.Quest.Guide.65056",
        "text": "(<textColor=[78:148:215:255]>비류하</>가 절대\n들켜서는 안 돼...!)"
    },
    "40065056": {
        "deprecated": false,
        "Dev_Desc": "65004_6",
        "id": 40065056,
        "alias": "Contents.Quest.Guide.65057",
        "text": "<textColor=[110:216:175:255]>분화구</>를\n어지럽히는 자는 누구냐!"
    },
    "40065057": {
        "deprecated": false,
        "Dev_Desc": "65004_6",
        "id": 40065057,
        "alias": "Contents.Quest.Guide.65058",
        "text": "(이런, 저자가\n<textColor=[215:94:94:255]>수용소 감독관</>인가...!)"
    },
    "40065058": {
        "deprecated": false,
        "Dev_Desc": "65007_1",
        "id": 40065058,
        "alias": "Contents.Quest.Guide.65059",
        "text": "(<textColor=[110:216:175:255]>수용소</>를 좀 더 찾아보도록 할까)"
    },
    "40065059": {
        "deprecated": false,
        "Dev_Desc": "65007_2_1_1",
        "id": 40065059,
        "alias": "Contents.Quest.Guide.65060",
        "text": "(일단은 <textColor=[110:216:175:255]>동편</>을 수색해보도록 하자)"
    },
    "40065060": {
        "deprecated": false,
        "Dev_Desc": "65007_2_1_2",
        "id": 40065060,
        "alias": "Contents.Quest.Guide.65061",
        "text": "(이 근방에 붙잡힌 아이는 없는 듯 해)"
    },
    "40065061": {
        "deprecated": false,
        "Dev_Desc": "65007_2_2_1",
        "id": 40065061,
        "alias": "Contents.Quest.Guide.65062",
        "text": "(반대편까지 찾아보고,\n없으면 다른 수를 내야겠어)"
    },
    "40070087": {
        "deprecated": false,
        "Dev_Desc": "전제하_피난처",
        "id": 40070087,
        "alias": "Contents.Quest.Guide.70087",
        "text": "전제하"
    },
    "40070027": {
        "deprecated": false,
        "Dev_Desc": "전제하_피난처",
        "id": 40070027,
        "alias": "Contents.Quest.Guide.70027",
        "text": "이런 무도한 놈들!\n죽어라 내... 내 <textColor=[255:204:102:255]>연인의 원수</>!"
    },
    "40070028": {
        "deprecated": false,
        "Dev_Desc": "PC",
        "id": 40070028,
        "alias": "Contents.Quest.Guide.70028",
        "text": "(아까는 <textColor=[255:204:102:255]>어머니의 원수</>라고 \n하지 않았나? 수상한걸)"
    },
    "40070029": {
        "deprecated": false,
        "Dev_Desc": "PC",
        "id": 40070029,
        "alias": "Contents.Quest.Guide.70029",
        "text": "(조금 전부터 <textColor=[78:148:215:255]>전제하</>의 \n목소리가 들리지 않는데... \n어디 간 거지?)"
    },
    "40070030": {
        "deprecated": false,
        "Dev_Desc": "PC",
        "id": 40070030,
        "alias": "Contents.Quest.Guide.70030",
        "text": "(아무리 찾아도 <textColor=[78:148:215:255]>전제하</>의 \n모습이 보이지 않는걸. \n돌아간 건가?)"
    },
    "40070031": {
        "deprecated": false,
        "Dev_Desc": "PC",
        "id": 40070031,
        "alias": "Contents.Quest.Guide.70031",
        "text": "(<textColor=[78:148:215:255]>전제하</>가 사라진 곳에서 \n그의 행방을 찾아봐야겠어)"
    },
    "40070032": {
        "deprecated": false,
        "Dev_Desc": "PC",
        "id": 40070032,
        "alias": "Contents.Quest.Guide.70032",
        "text": "(<textColor=[215:94:94:255]>검은새벽단원</>들의 말을 \n종합해보면 배신자는 \n<textColor=[110:216:175:255]>오염된 분화구</>에서 처단하는구나)"
    },
    "40070033": {
        "deprecated": false,
        "Dev_Desc": "PC",
        "id": 40070033,
        "alias": "Contents.Quest.Guide.70033",
        "text": "(일단 그곳으로 가서 \n동향을 살펴보자)"
    },
    "40070034": {
        "deprecated": false,
        "Dev_Desc": "PC",
        "id": 40070034,
        "alias": "Contents.Quest.Guide.70034",
        "text": "(저기 있는 건... 붙잡힌 사람인가?)"
    },
    "40070035": {
        "deprecated": false,
        "Dev_Desc": "PC",
        "id": 40070035,
        "alias": "Contents.Quest.Guide.70035",
        "text": "(<textColor=[255:204:102:255]>피난처의 위치</>로 \n교섭을 하려 했구나... \n악인이었지만 뒷맛이 쓰리네)"
    },
    "40070096": {
        "deprecated": false,
        "Dev_Desc": "PC",
        "id": 40070096,
        "alias": "Contents.Quest.Guide.70096",
        "text": "(<textColor=[215:94:94:255]>검은새벽단의 경비</>를 뚫으면서 \n탐색해봐야겠어)"
    }
}