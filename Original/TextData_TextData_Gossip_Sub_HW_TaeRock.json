    "30034507": {
        "deprecated": false,
        "Dev_Desc": "희생의 무덤_잡화 상인",
        "id": 30034507,
        "alias": "Contents.Gossip.34507",
        "text": "돈이고 나발이고\n오금 저려 죽겠네..."
    },
    "30034508": {
        "deprecated": false,
        "Dev_Desc": "희생의 무덤_잡화 상인",
        "id": 30034508,
        "alias": "Contents.Gossip.34508",
