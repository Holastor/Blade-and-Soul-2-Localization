{
    "40050000": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50000_0_pc 가이드",
        "id": 40050000,
        "alias": "Contents.Quest.Guide.50000",
        "text": "(그래! 나도 배우는\n마음으로 가보자)"
    },
    "40050001": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50000_1_pc 가이드",
        "id": 40050001,
        "alias": "Contents.Quest.Guide.50001",
        "text": "(수련장에 왔다.\n누구에게 말을 걸까?)"
    },
    "40050002": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50000_2_pc 가이드",
        "id": 40050002,
        "alias": "Contents.Quest.Guide.50002",
        "text": "(그래, <textColor=[78:148:215:255]>다른 단원</>과도\n이야기를 해보자)"
    },
    "40050003": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50000_3_pc 가이드",
        "id": 40050003,
        "alias": "Contents.Quest.Guide.50003",
        "text": "(<textColor=[110:216:175:255]>수수께끼 벌판</>으로 \n가보자!)"
    },
    "40050004": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50000_4_pc 가이드",
        "id": 40050004,
        "alias": "Contents.Quest.Guide.50004",
        "text": "(저기에 있다.\n가서 물어보자)"
    },
    "40050005": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50000_5_pc 가이드",
        "id": 40050005,
        "alias": "Contents.Quest.Guide.50005",
        "text": "(어쩌다 <textColor=[215:94:94:255]>어둑시니</>가 여기서\n자리를 잡게 된 걸까?)"
    },
    "40050006": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50000_6_pc 가이드",
        "id": 40050006,
        "alias": "Contents.Quest.Guide.50006",
        "text": "(충분히 정리된 것 같다.\n돌아가자!)"
    },
    "40050007": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50000_7_pc 가이드",
        "id": 40050007,
        "alias": "Contents.Quest.Guide.50007",
        "text": "(음... \n이제 슬슬 돌아가자)"
    },
    "40050008": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50001_1_pc 가이드",
        "id": 40050008,
        "alias": "Contents.Quest.Guide.50008",
        "text": "(<textColor=[78:148:215:255]>풍백문</>엔 \n다양한 자들이 있구나)"
    },
    "40050009": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50001_1_pc 가이드",
        "id": 40050009,
        "alias": "Contents.Quest.Guide.50009",
        "text": "(위험해 보이는데\n따라가 보자)"
    },
    "40050010": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50001_2_풍백문 다돈보 (은신처)",
        "id": 40050010,
        "alias": "Contents.Quest.Guide.50010",
        "text": "오셨군요?"
    },
    "40050011": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50001_3_pc 가이드",
        "id": 40050011,
        "alias": "Contents.Quest.Guide.50011",
        "text": "(<textColor=[215:94:94:255]>야금</>을 잡으며\n주변에서 지켜주자)"
    },
    "40050012": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50001_4_풍백문 다돈보 (은신처)",
        "id": 40050012,
        "alias": "Contents.Quest.Guide.50012",
        "text": "어이구! \n나 죽겠다는..."
    },
    "40050013": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50001_4_pc 가이드",
        "id": 40050013,
        "alias": "Contents.Quest.Guide.50013",
        "text": "(힘들어 보이는데...\n한 마리도 못 잡겠어)"
    },
    "40050014": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50001_5_풍백문 다돈보 (은신처)",
        "id": 40050014,
        "alias": "Contents.Quest.Guide.50014",
        "text": "은혜는 잊지 않겠습니다."
    },
    "40050015": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50001_6_풍백문 다돈보 (은신처)",
        "id": 40050015,
        "alias": "Contents.Quest.Guide.50015",
        "text": "감사합니다.\n여기선 돌아갈 수 있습니다."
    },
    "40050016": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50001_6_풍백문 다돈보 (은신처)",
        "id": 40050016,
        "alias": "Contents.Quest.Guide.50016",
        "text": "반성하고 훈련에\n매진하겠습니다."
    },
    "40050017": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50002_0_pc 가이드",
        "id": 40050017,
        "alias": "Contents.Quest.Guide.50017",
        "text": "(지역 주민을 돕는 건\n의미 있는 일이지)"
    },
    "40050018": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50002_1_pc 가이드",
        "id": 40050018,
        "alias": "Contents.Quest.Guide.50018",
        "text": "(<textColor=[78:148:215:255]>지미갓</>씨는 와 있군.\n가서 말을 걸자)"
    },
    "40050019": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50002_3_pc 가이드",
        "id": 40050019,
        "alias": "Contents.Quest.Guide.50019",
        "text": "(물리쳤다!)"
    },
    "40050020": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50003_0_pc 가이드",
        "id": 40050020,
        "alias": "Contents.Quest.Guide.50020",
        "text": "(오래간만에 봐서\n반가우니 같이 가자)"
    },
    "40050021": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50003_1_네이냥 (공통)",
        "id": 40050021,
        "alias": "Contents.Quest.Guide.50021",
        "text": "좋다냥! \n저기 야영지로 \n이동이다냥!"
    },
    "40050022": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50003_2_pc 가이드",
        "id": 40050022,
        "alias": "Contents.Quest.Guide.50022",
        "text": "(언제 야영지를 \n만든 거지?)"
    },
    "40050023": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50003_3_pc 가이드",
        "id": 40050023,
        "alias": "Contents.Quest.Guide.50023",
        "text": "(그냥 앞으로 가면 \n있을 것 같은데...)"
    },
    "40050024": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50003_4_네이냥 (공통)",
        "id": 40050024,
        "alias": "Contents.Quest.Guide.50024",
        "text": "찾았다냥! \n우리가 해냈다냥!"
    },
    "40050025": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50003_5_pc 가이드",
        "id": 40050025,
        "alias": "Contents.Quest.Guide.50025",
        "text": "(너무 위험한 일에 \n휘말리지 않길...)"
    },
    "40050026": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50004_0_pc 가이드",
        "id": 40050026,
        "alias": "Contents.Quest.Guide.50026",
        "text": "(따라 가보자)"
    },
    "40050027": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50004_1_pc 가이드",
        "id": 40050027,
        "alias": "Contents.Quest.Guide.50027",
        "text": "(우선 주변의 <textColor=[215:94:94:255]>어둑시니</>를\n제거해야겠군)"
    },
    "40050028": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50004_2_pc 가이드",
        "id": 40050028,
        "alias": "Contents.Quest.Guide.50028",
        "text": "(이제 좀 잠잠해졌군)"
    },
    "40050029": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50004_3_pc 가이드",
        "id": 40050029,
        "alias": "Contents.Quest.Guide.50029",
        "text": "(그런 사연이\n있었구나...)"
    },
    "40050030": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50100_0_pc 가이드",
        "id": 40050030,
        "alias": "Contents.Quest.Guide.50030",
        "text": "(의문을 해소하러\n숲으로 가보자!)"
    },
    "40050031": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50100_1_pc 가이드",
        "id": 40050031,
        "alias": "Contents.Quest.Guide.50031",
        "text": "(저기 있군. \n말을 걸어 보자)"
    },
    "40050032": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50100_2_pc 가이드",
        "id": 40050032,
        "alias": "Contents.Quest.Guide.50032",
        "text": "(식용은 잘 모르겠지만\n일단 도와주도록 하자)"
    },
    "40050033": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50100_3_pc 가이드",
        "id": 40050033,
        "alias": "Contents.Quest.Guide.50033",
        "text": "(이 정도면\n충분할 것 같은데)"
    },
    "40050034": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50100_4_pc 가이드",
        "id": 40050034,
        "alias": "Contents.Quest.Guide.50034",
        "text": "(좀 더 이해하기 쉬운\n연구를 해줬으면 좋겠다)"
    },
    "40050035": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50101_0_pc 가이드",
        "id": 40050035,
        "alias": "Contents.Quest.Guide.50035",
        "text": "(어디 한번\n같이 가볼까?)"
    },
    "40050036": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50101_1_박식물 가이드 (기본)",
        "id": 40050036,
        "alias": "Contents.Quest.Guide.50036",
        "text": "잘 찾아오셨군요!"
    },
    "40050037": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50101_2_pc 가이드",
        "id": 40050037,
        "alias": "Contents.Quest.Guide.50037",
        "text": "(그런 거라면\n얼마든지 할 수 있지)"
    },
    "40050038": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50101_3_pc 가이드",
        "id": 40050038,
        "alias": "Contents.Quest.Guide.50038",
        "text": "(주변 환경은\n정리된 것 같군)"
    },
    "40050039": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50101_3_박식물 가이드 (기본)",
        "id": 40050039,
        "alias": "Contents.Quest.Guide.50039",
        "text": "이쪽으로 오세요!"
    },
    "40050040": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50101_4_pc 가이드",
        "id": 40050040,
        "alias": "Contents.Quest.Guide.50040",
        "text": "(무엇이 나올지\n신경 쓰인다)"
    },
    "40050041": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50101_5_박식물 가이드 (기본)",
        "id": 40050041,
        "alias": "Contents.Quest.Guide.50041",
        "text": "특이한 흔적이 있는지\n찾아봐 주시겠어요?"
    },
    "40050042": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50101_6_pc 가이드",
        "id": 40050042,
        "alias": "Contents.Quest.Guide.50042",
        "text": "(딱히 찾은 게 없다)"
    },
    "40050043": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50101_6_박식물 가이드 (기본)",
        "id": 40050043,
        "alias": "Contents.Quest.Guide.50043",
        "text": "앗! 주변의 적들을\n제거해 주세요!"
    },
    "40050044": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50101_7_박식물 가이드 (기본)",
        "id": 40050044,
        "alias": "Contents.Quest.Guide.50044",
        "text": "조사 완료입니다!"
    },
    "40050045": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50101_8_박식물 가이드 (기본)",
        "id": 40050045,
        "alias": "Contents.Quest.Guide.50045",
        "text": "우리 모두 \n오래 살자구요!"
    },
    "40050046": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50102_0_pc 가이드",
        "id": 40050046,
        "alias": "Contents.Quest.Guide.50046",
        "text": "(무슨 사연이라도 \n있는 걸까?)"
    },
    "40050047": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50102_1_pc 가이드",
        "id": 40050047,
        "alias": "Contents.Quest.Guide.50047",
        "text": "(돈 안되는 연구라니\n사정이 딱해 보인다)"
    },
    "40050048": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50102_1_pc 가이드",
        "id": 40050048,
        "alias": "Contents.Quest.Guide.50048",
        "text": "(오늘 하루 정도는\n도와주도록 하자)"
    },
    "40050049": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50102_2_지질학자 원토지 (공통)",
        "id": 40050049,
        "alias": "Contents.Quest.Guide.50049",
        "text": "끝났다! 아까 도왔으면\n이미 쉬고 있을 거였어!"
    },
    "40050050": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50102_3_pc 가이드",
        "id": 40050050,
        "alias": "Contents.Quest.Guide.50050",
        "text": "(갑자기 공손한\n연구자가 되셨네?)"
    },
    "40050051": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50103_0_pc 가이드",
        "id": 40050051,
        "alias": "Contents.Quest.Guide.50051",
        "text": "(한번 따라가 볼까?)"
    },
    "40050052": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50103_1_지질학자 원토지 (공통)",
        "id": 40050052,
        "alias": "Contents.Quest.Guide.50052",
        "text": "여기야! 여기서 \n적들을 없애게나!"
    },
    "40050053": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50103_1_pc 가이드",
        "id": 40050053,
        "alias": "Contents.Quest.Guide.50053",
        "text": "(여기 좀 위험해 보이는데...)"
    },
    "40050054": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50103_2_지질학자 원토지 (공통)",
        "id": 40050054,
        "alias": "Contents.Quest.Guide.50054",
        "text": "잘했네. \n이쪽으로 와보게나!"
    },
    "40050055": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50103_3_pc 가이드",
        "id": 40050055,
        "alias": "Contents.Quest.Guide.50055",
        "text": "(여기 안쪽을 \n한번 찾아보죠)"
    },
    "40050056": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50103_4_지질학자 원토지 (공통)",
        "id": 40050056,
        "alias": "Contents.Quest.Guide.50056",
        "text": "여기 뭔가 있다!\n<textColor=[215:94:94:255]>청인족</>이다."
    },
    "40050057": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50103_4_지질학자 원토지 (공통)",
        "id": 40050057,
        "alias": "Contents.Quest.Guide.50057",
        "text": "납치범이냐!! \n가보자!"
    },
    "40050058": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50103_6_pc 가이드",
        "id": 40050058,
        "alias": "Contents.Quest.Guide.50058",
        "text": "(이렇게 떠나도 \n괜찮은 걸까?)"
    },
    "40050059": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50104_0_지질학자 원토지 (공통)",
        "id": 40050059,
        "alias": "Contents.Quest.Guide.50059",
        "text": "일단 나를 따라와."
    },
    "40050060": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50104_1_지질학자 원토지 (공통)",
        "id": 40050060,
        "alias": "Contents.Quest.Guide.50060",
        "text": "여기야! \n이쪽으로 와!"
    },
    "40050061": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50104_2_pc 가이드",
        "id": 40050061,
        "alias": "Contents.Quest.Guide.50061",
        "text": "(그거라면 쉽게\n도울 수 있겠다)"
    },
    "40050062": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50104_3_지질학자 원토지 (공통)",
        "id": 40050062,
        "alias": "Contents.Quest.Guide.50062",
        "text": "충분해. \n이걸로 조사해야겠다."
    },
    "40050063": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50105_0_pc 가이드",
        "id": 40050063,
        "alias": "Contents.Quest.Guide.50063",
        "text": "(주변의 적을 없애 주자)"
    },
    "40050064": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50105_1_pc 가이드",
        "id": 40050064,
        "alias": "Contents.Quest.Guide.50064",
        "text": "(<textColor=[78:148:215:255]>연구원</>을 따라가 보자)"
    },
    "40050065": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50105_2_pc 가이드",
        "id": 40050065,
        "alias": "Contents.Quest.Guide.50065",
        "text": "(안색이 안 좋은데... \n말을 걸어 보자)"
    },
    "40050066": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50105_3_pc 가이드",
        "id": 40050066,
        "alias": "Contents.Quest.Guide.50066",
        "text": "(말릴 수는 없고...\n주변을 정리해 주자)"
    },
    "40050067": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50105_4_pc 가이드",
        "id": 40050067,
        "alias": "Contents.Quest.Guide.50067",
        "text": "(다시 <textColor=[78:148:215:255]>연구원</>을 \n따라가 보자)"
    },
    "40050068": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50105_5_pc 가이드",
        "id": 40050068,
        "alias": "Contents.Quest.Guide.50068",
        "text": "(표정이 밝아 보인다)"
    },
    "40050069": {
        "deprecated": false,
        "Dev_Desc": "비옥서브 캐릭터 이름",
        "id": 40050069,
        "alias": "Contents.Quest.Guide.50069",
        "text": "풍백문 다돈보"
    },
    "40050070": {
        "deprecated": false,
        "Dev_Desc": "비옥서브 캐릭터 이름",
        "id": 40050070,
        "alias": "Contents.Quest.Guide.50070",
        "text": "개척바롱단 네이냥"
    },
    "40050071": {
        "deprecated": false,
        "Dev_Desc": "비옥서브 캐릭터 이름",
        "id": 40050071,
        "alias": "Contents.Quest.Guide.50071",
        "text": "동물학자 박식물"
    },
    "40050072": {
        "deprecated": false,
        "Dev_Desc": "비옥서브 캐릭터 이름",
        "id": 40050072,
        "alias": "Contents.Quest.Guide.50072",
        "text": "지질학자 원토지"
    },
    "40050073": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50005_0_pc 가이드",
        "id": 40050073,
        "alias": "Contents.Quest.Guide.50073",
        "text": "(무기 수집이라면\n도와줄 수 있겠지)"
    },
    "40050074": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50005_1_pc 가이드",
        "id": 40050074,
        "alias": "Contents.Quest.Guide.50074",
        "text": "(이 정도면 된 것 같다)"
    },
    "40050075": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50005_2_pc 가이드",
        "id": 40050075,
        "alias": "Contents.Quest.Guide.50075",
        "text": "(<textColor=[78:148:215:255]>풍백문</>이 아니라\n<textColor=[78:148:215:255]>금경철문</>이 어울릴 듯한데...)"
    },
    "40050076": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50006_0_pc 가이드",
        "id": 40050076,
        "alias": "Contents.Quest.Guide.50076",
        "text": "(바로 옆에 있는데\n왜 나한테 부탁을 하지?)"
    },
    "40050077": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50006_1_pc 가이드_1",
        "id": 40050077,
        "alias": "Contents.Quest.Guide.50077",
        "text": "(그럼 이곳의 <textColor=[215:94:94:255]>전갈</>로\n한번 확인해 볼까?)"
    },
    "40050078": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50006_1_pc 가이드_2",
        "id": 40050078,
        "alias": "Contents.Quest.Guide.50078",
        "text": "(<textColor=[255:204:102:255]>집게</>와 <textColor=[255:204:102:255]>꼬리</>를 \n수집해보자)"
    },
    "40050079": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50006_2_pc 가이드",
        "id": 40050079,
        "alias": "Contents.Quest.Guide.50079",
        "text": "(이거면 되겠어!)"
    },
    "40050080": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50006_3_pc 가이드",
        "id": 40050080,
        "alias": "Contents.Quest.Guide.50080",
        "text": "(아무 쓸모 없는\n논쟁이었다)"
    },
    "40050081": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50007_0_pc 가이드",
        "id": 40050081,
        "alias": "Contents.Quest.Guide.50081",
        "text": "(<textColor=[215:94:94:255]>만다라화</>가\n위협이 되는구나)"
    },
    "40050082": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50007_1_pc 가이드",
        "id": 40050082,
        "alias": "Contents.Quest.Guide.50082",
        "text": "(충분히 제거했다)"
    },
    "40050083": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50008_0_pc 가이드",
        "id": 40050083,
        "alias": "Contents.Quest.Guide.50083",
        "text": "(<textColor=[215:94:94:255]>잿빛 암각다귀</>가 줄어들면\n공포도 줄어들까?)"
    },
    "40050084": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50008_1_pc 가이드",
        "id": 40050084,
        "alias": "Contents.Quest.Guide.50084",
        "text": "(만나러 가볼까?)"
    },
    "40050085": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50008_2_pc 가이드_1",
        "id": 40050085,
        "alias": "Contents.Quest.Guide.50085",
        "text": "(없다. 못 나온 걸까?)"
    },
    "40050086": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50008_2_pc 가이드_2",
        "id": 40050086,
        "alias": "Contents.Quest.Guide.50086",
        "text": "(<textColor=[110:216:175:255]>해월지부</>로 가보자!)"
    },
    "40050087": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50009_0_pc 가이드",
        "id": 40050087,
        "alias": "Contents.Quest.Guide.50087",
        "text": "(<textColor=[78:148:215:255]>도화상전</>...\n괜찮은 걸까?)"
    },
    "40050088": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50009_1_pc 가이드_1",
        "id": 40050088,
        "alias": "Contents.Quest.Guide.50088",
        "text": "(강도와 경도를 \n비교하려고 하는 걸까?)"
    },
    "40050089": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50009_1_pc 가이드_2",
        "id": 40050089,
        "alias": "Contents.Quest.Guide.50089",
        "text": "(그럼 <textColor=[255:204:102:255]>이빨</>이 유리해 \n보이는데... 괜찮을까?)"
    },
    "40050090": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50009_2_pc 가이드",
        "id": 40050090,
        "alias": "Contents.Quest.Guide.50090",
        "text": "(돌아가도록 하자)"
    },
    "40050091": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50009_3_pc 가이드",
        "id": 40050091,
        "alias": "Contents.Quest.Guide.50091",
        "text": "(아무 의미 없는\n시간이었다)"
    },
    "40050092": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50106_0_pc 가이드",
        "id": 40050092,
        "alias": "Contents.Quest.Guide.50092",
        "text": "(<textColor=[255:204:102:255]>사묘정의 버섯</>을 \n얻어 보자)"
    },
    "40050093": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50106_1_pc 가이드_1",
        "id": 40050093,
        "alias": "Contents.Quest.Guide.50093",
        "text": "(만약 독버섯이 아니라면\n먹으면 되니까...)"
    },
    "40050094": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50106_1_pc 가이드_2",
        "id": 40050094,
        "alias": "Contents.Quest.Guide.50094",
        "text": "(다시 돌아가 보자)"
    },
    "40050095": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50106_2_pc 가이드_1",
        "id": 40050095,
        "alias": "Contents.Quest.Guide.50095",
        "text": "(어디 간 거지?)"
    },
    "40050096": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50106_2_pc 가이드_2",
        "id": 40050096,
        "alias": "Contents.Quest.Guide.50096",
        "text": "(옆 사람에게 물어보자)"
    },
    "40050097": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50106_3_pc 가이드",
        "id": 40050097,
        "alias": "Contents.Quest.Guide.50097",
        "text": "(버섯은 조심해서\n먹어야겠다)"
    },
    "40050098": {
        "deprecated": false,
        "Dev_Desc": "50003_3_PC",
        "id": 40050098,
        "alias": "Contents.Quest.Guide.50098",
        "text": "(가는 길에 \n검은새벽단을 처치하자)"
    },
    "40050099": {
        "deprecated": false,
        "Dev_Desc": "50003_4_PC",
        "id": 40050099,
        "alias": "Contents.Quest.Guide.50099",
        "text": "(조금만 더 가면\n동굴을 찾을 수 있겠지?)"
    },
    "40050100": {
        "deprecated": false,
        "Dev_Desc": "50200_0_PC",
        "id": 40050100,
        "alias": "Contents.Quest.Guide.50100",
        "text": "(<textColor=[215:94:94:255]>홍돈족</>에게서 <textColor=[78:148:215:255]>명혜단</>의 \n연구 자료를 되찾아 주자)"
    },
    "40050101": {
        "deprecated": false,
        "Dev_Desc": "50200_0_채란",
        "id": 40050101,
        "alias": "Contents.Quest.Guide.50101",
        "text": "어머머! 조심하세요!\n위험하면 도망치시구요!"
    },
    "40050102": {
        "deprecated": false,
        "Dev_Desc": "50200_1_채란",
        "id": 40050102,
        "alias": "Contents.Quest.Guide.50102",
        "text": "소협이 싸우는 모습을 보다가\n아주 좋은 계획이 떠올랐어요."
    },
    "40050103": {
        "deprecated": false,
        "Dev_Desc": "50200_1_채란",
        "id": 40050103,
        "alias": "Contents.Quest.Guide.50103",
        "text": "<textColor=[215:94:94:255]>홍돈족</>이 사람을 해코지\n못하게 무기를 빼앗는 거예요!"
    },
    "40050104": {
        "deprecated": false,
        "Dev_Desc": "50200_2_채란",
        "id": 40050104,
        "alias": "Contents.Quest.Guide.50104",
        "text": "됐어요! 충분해요!!"
    },
    "40050105": {
        "deprecated": false,
        "Dev_Desc": "50200_3_채란",
        "id": 40050105,
        "alias": "Contents.Quest.Guide.50105",
        "text": "그럼 <textColor=[110:216:175:255]>붉은 숲길</> 입구에서 만나요!"
    },
    "40050106": {
        "deprecated": false,
        "Dev_Desc": "50200_4_채란",
        "id": 40050106,
        "alias": "Contents.Quest.Guide.50106",
        "text": "으아악! 도와줘요!"
    },
    "40050107": {
        "deprecated": false,
        "Dev_Desc": "50200_4_채란",
        "id": 40050107,
        "alias": "Contents.Quest.Guide.50107",
        "text": "(<textColor=[78:148:215:255]>채란</>이 포위당한 것 같다!)"
    },
    "40050108": {
        "deprecated": false,
        "Dev_Desc": "50200_5_홍돈삼",
        "id": 40050108,
        "alias": "Contents.Quest.Guide.50108",
        "text": "모두 그만하셔. \n저 인간들과 할 이야기가 있으셔."
    },
    "40050109": {
        "deprecated": false,
        "Dev_Desc": "50200_5_홍돈삼",
        "id": 40050109,
        "alias": "Contents.Quest.Guide.50109",
        "text": "거기 인간, 이리로 오셔."
    },
    "40050110": {
        "deprecated": false,
        "Dev_Desc": "50200_6_홍돈삼",
        "id": 40050110,
        "alias": "Contents.Quest.Guide.50110",
        "text": "이번이 정말 마지막이셔!!!\n거기 있는 <textColor=[255:204:102:255]>비석</>도 읽고 가셔."
    },
    "40050111": {
        "deprecated": false,
        "Dev_Desc": "50201_1_권도이",
        "id": 40050111,
        "alias": "Contents.Quest.Guide.50111",
        "text": "명탐정 <textColor=[78:148:215:255]>권도이</>! 출동이다!"
    },
    "40050112": {
        "deprecated": false,
        "Dev_Desc": "50201_2_권도이",
        "id": 40050112,
        "alias": "Contents.Quest.Guide.50112",
        "text": "세상에! 엄청나다!!\n내 조수 완전 강하구나?"
    },
    "40050113": {
        "deprecated": false,
        "Dev_Desc": "50201_2_권도이",
        "id": 40050113,
        "alias": "Contents.Quest.Guide.50113",
        "text": "좋아! 우리 모험을 떠나자!"
    },
    "40050114": {
        "deprecated": false,
        "Dev_Desc": "50201_2_PC",
        "id": 40050114,
        "alias": "Contents.Quest.Guide.50114",
        "text": "(<textColor=[78:148:215:255]>꼬마</>가 뛰어간다!\n어서 따라가자!)"
    },
    "40050115": {
        "deprecated": false,
        "Dev_Desc": "50201_4_PC",
        "id": 40050115,
        "alias": "Contents.Quest.Guide.50115",
        "text": "(<textColor=[78:148:215:255]>꼬마</>의 울분이 느껴진다. \n주변에서 <textColor=[215:94:94:255]>홍돈족</>을 내쫓자)"
    },
    "40050116": {
        "deprecated": false,
        "Dev_Desc": "50201_4_권도이",
        "id": 40050116,
        "alias": "Contents.Quest.Guide.50116",
        "text": "조수야! 힘내! 조심해!"
    },
    "40050117": {
        "deprecated": false,
        "Dev_Desc": "50201_5_권도이",
        "id": 40050117,
        "alias": "Contents.Quest.Guide.50117",
        "text": "으, 으아악! \n<textColor=[215:94:94:255]>빨간 돼지</>들이 날 공격한다!!"
    },
    "40050118": {
        "deprecated": false,
        "Dev_Desc": "50201_5_홍돈족",
        "id": 40050118,
        "alias": "Contents.Quest.Guide.50118",
        "text": "꼬마, 지금 뭐라고 했셔?!!!!"
    },
    "40050119": {
        "deprecated": false,
        "Dev_Desc": "50201_5_PC",
        "id": 40050119,
        "alias": "Contents.Quest.Guide.50119",
        "text": "(<textColor=[78:148:215:255]>꼬마</>가 위험하다!)"
    },
    "40050120": {
        "deprecated": false,
        "Dev_Desc": "50201_6_권도이",
        "id": 40050120,
        "alias": "Contents.Quest.Guide.50120",
        "text": "내 조수 최고다! \n엄청 강해!"
    },
    "40050121": {
        "deprecated": false,
        "Dev_Desc": "50201_7_PC",
        "id": 40050121,
        "alias": "Contents.Quest.Guide.50121",
        "text": "(또 달려간다.\n놓치기 전에 어서 가자)"
    },
    "40050122": {
        "deprecated": false,
        "Dev_Desc": "50201_8_권도이",
        "id": 40050122,
        "alias": "Contents.Quest.Guide.50122",
        "text": "비밀 장소는 바로 저 <textColor=[110:216:175:255]>폭포 뒤</>야! \n조심해서 따라와!!"
    },
    "40050123": {
        "deprecated": false,
        "Dev_Desc": "50201_8_PC",
        "id": 40050123,
        "alias": "Contents.Quest.Guide.50123",
        "text": "(세상에! 위험해 보인다.\n어서 따라가자!)"
    },
    "40050124": {
        "deprecated": false,
        "Dev_Desc": "50201_9_권도이",
        "id": 40050124,
        "alias": "Contents.Quest.Guide.50124",
        "text": "짜잔! 완전 수상하지!"
    },
    "40050125": {
        "deprecated": false,
        "Dev_Desc": "50201_10_권도이",
        "id": 40050125,
        "alias": "Contents.Quest.Guide.50125",
        "text": "여기 길은 내가 더 잘 알지롱!\n먼저 간다!"
    },
    "40050126": {
        "deprecated": false,
        "Dev_Desc": "50202_1_PC",
        "id": 40050126,
        "alias": "Contents.Quest.Guide.50126",
        "text": "(<textColor=[110:216:175:255]>단풍노을 골짜기</>로 가자)"
    },
    "40050127": {
        "deprecated": false,
        "Dev_Desc": "50202_2_비류하",
        "id": 40050127,
        "alias": "Contents.Quest.Guide.50127",
        "text": "소협! 여깁니다."
    },
    "40050128": {
        "deprecated": false,
        "Dev_Desc": "50202_3_PC",
        "id": 40050128,
        "alias": "Contents.Quest.Guide.50128",
        "text": "(우선 <textColor=[110:216:175:255]>수수께끼 벌판</>부터 \n정리 하는게 좋겠어)"
    },
    "40050129": {
        "deprecated": false,
        "Dev_Desc": "50202_4_PC",
        "id": 40050129,
        "alias": "Contents.Quest.Guide.50129",
        "text": "(이정도면 충분해.\n이제 <textColor=[110:216:175:255]>벌목지</>로 가보자)"
    },
    "40050130": {
        "deprecated": false,
        "Dev_Desc": "50202_5_비류하",
        "id": 40050130,
        "alias": "Contents.Quest.Guide.50130",
        "text": "이야, 역시 대단하십니다."
    },
    "40050131": {
        "deprecated": false,
        "Dev_Desc": "50203_1_고만후",
        "id": 40050131,
        "alias": "Contents.Quest.Guide.50131",
        "text": "정말입니까? \n그럼 사양하지 않겠습니다."
    },
    "40050132": {
        "deprecated": false,
        "Dev_Desc": "50203_1_PC",
        "id": 40050132,
        "alias": "Contents.Quest.Guide.50132",
        "text": "(<textColor=[78:148:215:255]>노인</>을 도와 <textColor=[215:94:94:255]>시귀</>에게 \n안식을 찾아주자)"
    },
    "40050133": {
        "deprecated": false,
        "Dev_Desc": "50203_2_PC",
        "id": 40050133,
        "alias": "Contents.Quest.Guide.50133",
        "text": "(음? 이건! 병사들의 \n이름이 적힌 인식표다!)"
    },
    "40050134": {
        "deprecated": false,
        "Dev_Desc": "50203_2_PC",
        "id": 40050134,
        "alias": "Contents.Quest.Guide.50134",
        "text": "(이걸 모아다 주면 \n<textColor=[78:148:215:255]>노인</>의 마음이 조금\n편안해질지도 모르겠군)"
    },
    "40050135": {
        "deprecated": false,
        "Dev_Desc": "50203_3_PC",
        "id": 40050135,
        "alias": "Contents.Quest.Guide.50135",
        "text": "(후, 제법 모았다.\n<textColor=[78:148:215:255]>노인</>에게 돌아가자)"
    },
    "40050136": {
        "deprecated": false,
        "Dev_Desc": "50203_6_PC",
        "id": 40050136,
        "alias": "Contents.Quest.Guide.50136",
        "text": "(모두 처치했다.\n<textColor=[78:148:215:255]>노인</>이 안전하게 돌아가길...)"
    },
    "40050137": {
        "deprecated": false,
        "Dev_Desc": "50204_0_PC",
        "id": 40050137,
        "alias": "Contents.Quest.Guide.50137",
        "text": "<textColor=[255:204:102:255]>사슴</>을 구출하는 동안\n<textColor=[215:94:94:255]>투엽단</>을 처리해줘!"
    },
    "40050138": {
        "deprecated": false,
        "Dev_Desc": "50204_1_마파람",
        "id": 40050138,
        "alias": "Contents.Quest.Guide.50138",
        "text": "거기 안쪽에 <textColor=[255:204:102:255]>사슴</>이\n쓰러져 있어! 부탁해!"
    },
    "40050139": {
        "deprecated": false,
        "Dev_Desc": "50204_2_마파람",
        "id": 40050139,
        "alias": "Contents.Quest.Guide.50139",
        "text": "여기 언덕 위쪽에도 간악한 \n<textColor=[215:94:94:255]>투엽단</> 놈들이 진을 치고 있어!"
    },
    "40050140": {
        "deprecated": false,
        "Dev_Desc": "50204_3_마파람",
        "id": 40050140,
        "alias": "Contents.Quest.Guide.50140",
        "text": "이번에도 부탁할게."
    },
    "40050141": {
        "deprecated": false,
        "Dev_Desc": "50204_4_마파람",
        "id": 40050141,
        "alias": "Contents.Quest.Guide.50141",
        "text": "됐어! 이정도면 충분해."
    },
    "40050142": {
        "deprecated": false,
        "Dev_Desc": "50205_1_링링",
        "id": 40050142,
        "alias": "Contents.Quest.Guide.50142",
        "text": "우와앙! 찾았다냥!"
    },
    "40050143": {
        "deprecated": false,
        "Dev_Desc": "50205_2_링링",
        "id": 40050143,
        "alias": "Contents.Quest.Guide.50143",
        "text": "<textColor=[78:148:215:255]>링링</>만 믿고 따라 오라냥!"
    },
    "40050144": {
        "deprecated": false,
        "Dev_Desc": "50205_3_링링",
        "id": 40050144,
        "alias": "Contents.Quest.Guide.50144",
        "text": "분명 이 근처다냥!\n이번에도 날 지켜달라냥!"
    },
    "40050145": {
        "deprecated": false,
        "Dev_Desc": "50205_4_PC",
        "id": 40050145,
        "alias": "Contents.Quest.Guide.50145",
        "text": "(음? 무슨 냄새지...?\n따뜻한... 소금 냄새?)"
    },
    "40050146": {
        "deprecated": false,
        "Dev_Desc": "50205_5_PC",
        "id": 40050146,
        "alias": "Contents.Quest.Guide.50146",
        "text": "(이게 설마 <textColor=[255:204:102:255]>물꼬리 풀</>?\n<textColor=[78:148:215:255]>링링</>에게 보여주자)"
    },
    "40050147": {
        "deprecated": false,
        "Dev_Desc": "50206_0_신호언",
        "id": 40050147,
        "alias": "Contents.Quest.Guide.50147",
        "text": "대협! 잘 부탁드립니다!"
    },
    "40050148": {
        "deprecated": false,
        "Dev_Desc": "50206_0_PC",
        "id": 40050148,
        "alias": "Contents.Quest.Guide.50148",
        "text": "(우선 <textColor=[215:94:94:255]>창도단 덩치</>를 \n처치하여 <textColor=[255:204:102:255]>금낭</>부터 찾자)"
    },
    "40050149": {
        "deprecated": false,
        "Dev_Desc": "50206_1_PC",
        "id": 40050149,
        "alias": "Contents.Quest.Guide.50149",
        "text": "(<textColor=[255:204:102:255]>금낭</>을 찾았다! <textColor=[215:94:94:255]>감시병</>이 \n가져갔다는 <textColor=[255:204:102:255]>예물</>도 찾아보자)"
    },
    "40050150": {
        "deprecated": false,
        "Dev_Desc": "50206_2_PC",
        "id": 40050150,
        "alias": "Contents.Quest.Guide.50150",
        "text": "(이거로군! 딱 봐도 \n<textColor=[255:204:102:255]>신혼부부의 예물</>처럼 보인다)"
    },
    "40050151": {
        "deprecated": false,
        "Dev_Desc": "50206_2_PC",
        "id": 40050151,
        "alias": "Contents.Quest.Guide.50151",
        "text": "(그럼 이제 <textColor=[255:204:102:255]>여행 가방</>만\n찾으면 되겠군. 저쪽으로 가보자)"
    },
    "40050152": {
        "deprecated": false,
        "Dev_Desc": "50206_3_PC",
        "id": 40050152,
        "alias": "Contents.Quest.Guide.50152",
        "text": "(저기 있다! 우선 주변의 \n<textColor=[215:94:94:255]>창도단</>부터 정리해야겠네)"
    },
    "40050153": {
        "deprecated": false,
        "Dev_Desc": "50206_4_동충",
        "id": 40050153,
        "alias": "Contents.Quest.Guide.50153",
        "text": "니들 지금 뭐하냐?"
    },
    "40050154": {
        "deprecated": false,
        "Dev_Desc": "50206_5_PC",
        "id": 40050154,
        "alias": "Contents.Quest.Guide.50154",
        "text": "(... 무척 강해보인다. \n<textColor=[255:204:102:255]>여행 가방</>을 회수하여 돌아가자)"
    },
    "40050155": {
        "deprecated": false,
        "Dev_Desc": "50206_6_PC",
        "id": 40050155,
        "alias": "Contents.Quest.Guide.50155",
        "text": "(휴, 힘들었다.\n<textColor=[78:148:215:255]>부부</>에게 돌아가자)"
    },
    "40050156": {
        "deprecated": false,
        "Dev_Desc": "서브_NPC_채란",
        "id": 40050156,
        "alias": "Contents.Quest.Guide.50156",
        "text": "명혜단 채란"
    },
    "40050157": {
        "deprecated": false,
        "Dev_Desc": "서브_NPC_홍돈삼",
        "id": 40050157,
        "alias": "Contents.Quest.Guide.50157",
        "text": "홍돈족 간부 홍돈삼"
    },
    "40050158": {
        "deprecated": false,
        "Dev_Desc": "서브_NPC_권도이",
        "id": 40050158,
        "alias": "Contents.Quest.Guide.50158",
        "text": "꼬마 탐정 권도이"
    },
    "40050159": {
        "deprecated": false,
        "Dev_Desc": "서브_NPC_홍돈족",
        "id": 40050159,
        "alias": "Contents.Quest.Guide.50159",
        "text": "화난 홍돈족"
    },
    "40050160": {
        "deprecated": false,
        "Dev_Desc": "서브_NPC_비류하",
        "id": 40050160,
        "alias": "Contents.Quest.Guide.50160",
        "text": "풍백문 비류하"
    },
    "40050161": {
        "deprecated": false,
        "Dev_Desc": "서브_NPC_고만후",
        "id": 40050161,
        "alias": "Contents.Quest.Guide.50161",
        "text": "수양대원 고만후"
    },
    "40050162": {
        "deprecated": false,
        "Dev_Desc": "서브_NPC_마파람",
        "id": 40050162,
        "alias": "Contents.Quest.Guide.50162",
        "text": "야생 보전회 마파람"
    },
    "40050163": {
        "deprecated": false,
        "Dev_Desc": "서브_NPC_링링",
        "id": 40050163,
        "alias": "Contents.Quest.Guide.50163",
        "text": "영교원 링링"
    },
    "40050164": {
        "deprecated": false,
        "Dev_Desc": "서브_NPC_신호언",
        "id": 40050164,
        "alias": "Contents.Quest.Guide.50164",
        "text": "여행객 신호언"
    },
    "40050165": {
        "deprecated": false,
        "Dev_Desc": "서브_NPC_동충",
        "id": 40050165,
        "alias": "Contents.Quest.Guide.50165",
        "text": "청면귀 동충"
    },
    "40050166": {
        "deprecated": false,
        "Dev_Desc": "50207_0_PC",
        "id": 40050166,
        "alias": "Contents.Quest.Guide.50166",
        "text": "(<textColor=[255:204:102:255]>어두운 기운</>이라고?\n아무래도 확인해봐야겠어)"
    },
    "40050167": {
        "deprecated": false,
        "Dev_Desc": "50207_2_PC",
        "id": 40050167,
        "alias": "Contents.Quest.Guide.50167",
        "text": "(<textColor=[255:204:102:255]>불마의 기운</>이 강렬히 느껴진다.\n마음의 준비를 철저히 해두자)"
    },
    "40050168": {
        "deprecated": false,
        "Dev_Desc": "50201_0_PC",
        "id": 40050168,
        "alias": "Contents.Quest.Guide.50168",
        "text": "(<textColor=[78:148:215:255]>꼬마</>와 얘기해보자)"
    },
    "40050169": {
        "deprecated": false,
        "Dev_Desc": "50202_0_PC",
        "id": 40050169,
        "alias": "Contents.Quest.Guide.50169",
        "text": "(저기 저 <textColor=[78:148:215:255]>문파원</>에게\n물어보는 게 좋겠군)"
    },
    "40050170": {
        "deprecated": false,
        "Dev_Desc": "50203_5_PC",
        "id": 40050170,
        "alias": "Contents.Quest.Guide.50170",
        "text": "(<textColor=[110:216:175:255]>비옥 폐허</>로 가서\n<textColor=[215:94:94:255]>불마자</>를 처치하자)"
    },
    "40050171": {
        "deprecated": false,
        "Dev_Desc": "50205_0_PC",
        "id": 40050171,
        "alias": "Contents.Quest.Guide.50171",
        "text": "(<textColor=[78:148:215:255]>링링</>이 약초 채집에\n전념할 수 있도록 \n주변을 정리하자)"
    }
}