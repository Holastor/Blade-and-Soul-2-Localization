    "20000910": {
        "deprecated": false,
        "Dev_Desc": "900_7_아랑3",
        "id": 20000910,
        "alias": "Contents.Quest.Dialogue.910",
        "text": "I think we've taken care of all the Hollows...\nI was surprised to see Ecizer here."
    },
    "20000911": {
        "deprecated": false,
        "Dev_Desc": "900_7_아랑3",
        "id": 20000911,
        "alias": "Contents.Quest.Dialogue.911",
        "text": "You might have died if I hadn't showed up on time."
    },
    "20000912": {
        "deprecated": false,
        "Dev_Desc": "900_7_PC",
        "id": 20000912,
        "alias": "Contents.Quest.Dialogue.912",
        "text": "(I could've handled it on my own.)"
    },
    "20000913": {
        "deprecated": false,
        "Dev_Desc": "900_7_아랑3",
        "id": 20000913,
        "alias": "Contents.Quest.Dialogue.913",
        "text": "Of course you could've.\nWell, I need to get ready now, so I'll catch up with you later."
    },
    "20000914": {
        "deprecated": false,
        "Dev_Desc": "900_7_아랑3",
        "id": 20000914,
        "alias": "Contents.Quest.Dialogue.914",
        "text": "I saw the arena entrance over there.\nSee you inside!"
    },
    "20000915": {
        "deprecated": false,
        "Dev_Desc": "900_8_PC",
        "id": 20000915,
        "alias": "Contents.Quest.Dialogue.915",
        "text": "(Hello.\nIs this the way to the Martial Arts Arena?)"
    },
    "20000916": {
        "deprecated": false,
        "Dev_Desc": "900_8_아랑4",
        "id": 20000916,
        "alias": "Contents.Quest.Dialogue.916",
        "text": "Yes! You're a participant, I take it?\nYou're pretty early."
    },
    "20000917": {
        "deprecated": false,
        "Dev_Desc": "900_8_아랑4",
        "id": 20000917,
        "alias": "Contents.Quest.Dialogue.917",
        "text": "It will be a while until the championship starts.\nWhy don't you warm up around here while you wait?"
    },
    "20000918": {
        "deprecated": false,
        "Dev_Desc": "900_8_PC",
        "id": 20000918,
        "alias": "Contents.Quest.Dialogue.918",
        "text": "(That sounds like a plan.\nOne thing, though... could I borrow a weapon? I didn't bring my own."
    },
    "20000919": {
        "deprecated": false,
        "Dev_Desc": "900_8_아랑4",
        "id": 20000919,
        "alias": "Contents.Quest.Dialogue.919",
        "text": "Sure! We lend swords to the participants.\nThey're not the best swords, but sturdy enough for a warm-up!"
    },
    "20000920": {
        "deprecated": false,
        "Dev_Desc": "900_8_아랑4",
        "id": 20000920,
        "alias": "Contents.Quest.Dialogue.920",
        "text": "I will see you in a bit!"
    }
