    "40045035": {
        "deprecated": false,
        "Dev_Desc": "비옥스페셜_45103_2_pc 가이드",
        "id": 40045035,
        "alias": "Contents.Quest.Guide.45035",
        "text": "(I'll be able to find them\nif I investigate the area.)"
    },
    "40045036": {
        "deprecated": false,
        "Dev_Desc": "비옥스페셜_45103_3_pc 가이드",
        "id": 40045036,
        "alias": "Contents.Quest.Guide.45036",
        "text": "(Found it. There they are.)"
    },
